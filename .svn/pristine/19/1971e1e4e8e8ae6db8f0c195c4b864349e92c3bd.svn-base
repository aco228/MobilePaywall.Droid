#contentVideos{
  padding-top:10px;
}
.row{margin:0px!important;}
.content_video{
  /*margin-top:10px;*/
  float:left;
  /*background-image:url('http://d12tusb9bq3y6m.cloudfront.net/wp-content/uploads/2014/02/powerful-photos-13.jpg');*/
  /*background-size:cover;*/
  /*background-position:center;*/
  cursor:hand; cursor:pointer;
  padding:5px!important;
  overflow:hidden;
  /*transition:all 1s;*/
}

.content_video img{
  max-width:100%; height:auto;
}
.content_video_inside{
  position:relative!important;
  border:5px solid rgb(250,0,46);
  background-color:rgb(23,23,23);
  min-height:100px;
  /*overflow:hidden;*/
}
#conten_video_ofDay_in{ max-height: 350px;  overflow:hidden; }
.content_video_inside img{ width:100%; }
.content_video_playIcon{
  position:absolute; height:100%; width:100%;
  pointer-events:none;
  background-image:url('../../Images/Black/Play.png');
  background-size: initial;
  background-repeat:no-repeat;
  background-size: 50px;
  background-position:center;
  opacity:.8;
}
.content_video_title{
  position:absolute;
  width:90%; margin-left:0%;
  bottom:0px; 
  font-size:13px;
  background-color:rgb(60,60,60);
  background:linear-gradient(to right, rgba(60,60,60, .9), rgba(60,60,60,0));
  padding:5px 10px;
  color:#FFF;
  text-shadow:1px 1px 3px #000;
}


