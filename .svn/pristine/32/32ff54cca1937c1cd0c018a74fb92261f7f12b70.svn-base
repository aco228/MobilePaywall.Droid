.contentBase {
}
.videoContentGroup{
  padding:5px;
  padding-top:20px;
  border: 1px solid #1976D2;
  margin-bottom:15px;
  background-color: #94AAD0;
  border-radius:15px;
  cursor:hand; cursor:pointer;
}
.videoContentGroup_hidden{/* opacity:.5; */}
.videoContentWrap {
  opacity:.8;
  /* pointer-events:none; */
}
.videoContent {
  height:55px;
  border-radius:5px;
  background-size:contain;
  background-repeat:no-repeat;
  background-position: left;
  line-height:55px;
  text-align:center;
  margin: 3px 0px;
  font-weight:bold;
  background-color: #F0F4FF;
}

.ondrop {
  border:1px solid red;
  opacity:1;
}
.ondragg{ opacity: .5; }
#sortContentUpdate{}
.videoContent_hidden{ opacity: .5};