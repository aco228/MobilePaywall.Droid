body {
  margin:0px; padding:0px!important;
  background-color:#063C51!important;
  margin:0px!important;
  color: #DADADA!important;
  /*font-family: 'Ubuntu Condensed', sans-serif!important;*/
  font-family: 'Dosis', sans-serif!important;
}
/*#wrapper{
  width:100%;
  min-height:100%;
  position:relative;
}*/

/*
  ======================================================
  CONSTANTS

*/
.header {
  color: white;
  font-size: 20px;
  margin-top: 0px;
  margin-bottom: 15px;
  text-transform: uppercase;
  text-align: center;
  background-color: #52A1AA;
  padding-top: 10px;
  padding-bottom: 10px;
  letter-spacing: 7px;
}
.header1{ font-size:17px; border:none; }

a{
  color:inherit!important;
  font:inherit!important;
  text-decoration:inherit;
}

.btn:hover{color:inherit!important;}

/*
  ======================================================
  CONTENT

*/

#page_content{
  padding:30px;
  padding-top:10px;
  overflow:hidden;
}

.aditionalText{
  margin:0px!important;
  padding: 7px 10px;
  margin-top:15px;
  font-size:16px; 
  text-align:center;
  padding-top: 15px;
}
.btn{
  background: linear-gradient(rgb(250,0,46), rgb(249,0,37));
  background-color:rgb(250,0,46);
  border:none;
  padding:10px 30px;
  color:#FFF;
  font:inherit;
  font-size:14px;
  cursor:hand; cursor:pointer;
  text-shadow:1px 1px 1px rgba(0,0,0,.5);
  border-radius:3px;
}

input[type=submit], .submitBtn{
  text-align:center;
  background: linear-gradient(rgb(250,0,46), rgb(249,0,37));
  background-color:rgb(250,0,46);
  font:inherit;
  color:#FFF;
  width:100%;
  margin-top:15px;
  padding:13px 0px;
  border-radius:5px;
  border:none;
  text-transform:uppercase;
  font-weight:bold;
  cursor:hand; cursor:pointer;
}

/* quiz */
#quizQuestionData, #quizQuestionDataOLD { transition:all .4s; }
#quizQuestionWait { transition:all .4s; }
.quizAnswer{
  transition:all .5s;
  padding:5px;
  background-color: black;
  margin:5px;
  cursor:hand;
  cursor: pointer;
}
.quizSubmitNotActive{ pointer-event:none; opacity:.2; }

.quizAnswerClicked{background-color: #adadad; color:black; }
.quizAnswerCorect{background-color: #1d651d;}
.quizAnswerWrong{background-color: #883939;}