#contentVideos{
  padding-top:10px;
}
.row{margin:0px;}
.content_video{
  /*margin-top:10px;*/
  float:left;
  /* Border is set from ContentHome.js (Scripts/Template/ContentHome.js) */
  /* Margin is set from ContentHome.js (Scripts/Template/ContentHome.js) */
  /*background-image:url('http://d12tusb9bq3y6m.cloudfront.net/wp-content/uploads/2014/02/powerful-photos-13.jpg');*/
  /*background-size:cover;*/
  /*background-position:center;*/
  cursor:hand; cursor:pointer;
  padding:5px!important;
  overflow:hidden;
  /*transition:all 1s;*/
}

.content_video img{
  max-width:100%; height:auto;
}
.content_video_inside{
  position:relative!important;
  border:5px solid rgb(250,0,46);
  background-color:rgb(23,23,23);
  min-height:100px;
  /*overflow:hidden;*/
}
#conten_video_ofDay_in{ max-height:350px; overflow:hidden; }
.content_video_inside img{ width:100%; }
.content_video_playIcon{
  position:absolute; height:100%; width:100%;
  pointer-events:none;
  background-image:url('../../Images/Red/Play.png');
  background-size: initial;
  background-repeat:no-repeat;
  background-size: 30px;
  background-position:center;
  opacity:.8;
}
.video_title{ 
  position:absolute;
  bottom:5px; left:5px; padding:5px;background-color:rgba(0,0,0,.8);
}
.content_video_title{
  position:absolute;
  width:90%; margin-left:0%;
  bottom:-1px; 
  font-size:13px;
  background-color:rgb(250,0,46);
  background:linear-gradient(to right, rgba(250,0,46, .9), rgba(250,0,46,0));
  padding:5px 10px;
  color:#FFF;
  text-shadow:1px 1px 3px #000;
}



#contentVideoControls{}
.content_videoControl{}
#controlLoadMore{
  cursor:hand; cursor:pointer;
  text-align:center;
  background-color:rgb(60,60,60);
  padding:10px 0px 10px 0px;
  border-radius:4px;
  color:#FFF;
  font-size:18px;
  margin-top:10px;
  margin-bottom:10px;
}

.quizIcon{
  background-image:url('../../Images/QuizContent/icon.png');
    background-size:contain;
    background-repeat:no-repeat;
    background-position:center;
}

.quizTitle{
  background-color:#00c0ff;
  color:white;
  text-transform:uppercase;
      font-size: 16px;
    font-weight: bold;
    float:left;
    width: 100%;
}

.quizArrow{
  background-image:url('../../Images/QuizContent/arrow_right.png');
    background-size:contain;
    background-repeat:no-repeat;
    background-position:center;
    margin: 0 auto;
}

.quizText{  
  font-size: 16px;
    font-weight: bold;
    color:#0a3260;
}

.quizQuestions{
  background-color:white;
  float:left;
}

.quizName{
  clear:both;
  margin-bottom: 20px;
}

.quizTable{
    width: 100%;
    border-radius: 5px;
    border: 0px solid black;
    overflow: hidden;
}

.quizTable td{
    background-color:white;
    width:47%;

}

.quizTable td:last-of-type{
    background-color:#00c0ff;
    width:6%;
    padding:3px;
}

.quizQuestions, .quizTitle{
    padding:10px;
    text-align: center;
    width:100%;
} 

.quizNumber{    
    font-size: 65px;
    color: #00c0ff;
}

#quizTimer {
    font-size: 125px;
    text-align: center;
    color: white;
        margin-top: -40px;
    margin-bottom: -30px;
}

.question {
    background-color: #00c0ff;
    border: 0px solid black;
    border-radius: 5px;
    margin: 0px 5px!important;
    padding: 5px;
    text-align: center;
    font-size: 17px;
    color: black;
}

.answerHolder {
    width: 50%;
    margin: 0;
    float: left;
    padding: 3px;
    border: 0 solid black;
    overflow: hidden;
    border-radius: 16px;
        display: table;
}

.answerNumber {
    padding: 5px;
    background-color: #00c0ff;
    font-size: 32px;
    width: 55px;
    min-height: 55px;
    height: inherit;
    color: white;
    text-align: center;
    overflow: hidden;
    display: table-cell;
    vertical-align: middle;
}