#contentVideoOfDay{}
#conten_video_ofDay{}


/*
  VIDEOS
*/

#contentVideos{
  padding-top:10px;
}
.content_video{
  float:left;
  margin-top:5px;
  /*background-position:center;*/
  cursor:hand; cursor:pointer;
  /*transition:all 1s;*/
  overflow:hidden;
}
.content_video_inside{
  position:relative;
  background-color:#333;
  border:5px solid rgb(255, 86, 172);
  min-height:100px;
  /*padding:3px;*/
  /*overflow:hidden;*/
}
#conten_video_ofDay_in{max-height:350px; overflow:hidden; }
.content_video_inside img{ width:100%; }
.content_video img{
  max-width:100%; height:auto;
}

.content_video_playIcon{
  position:absolute; height:100%; width:100%;
  pointer-events:none;
  background-size: initial;
  background-repeat:no-repeat;
  background-size: 30px;
  background-position:center;
  opacity:.8;
}
.__contentIcon_video{ background-image:url('../../Images/Pink/play_btn.png')!important; }

.content_video_title{
  position:absolute;
  bottom:5px; left:5px;
  font-size:13px;
  background-color:#FF55AD;
  padding:3px;
  color:#FFF;
  text-shadow:1px 1px 3px #000;
  box-shadow:0px 0px 30px #000;
}



#contentVideoControls{}
.content_videoControl{}
#controlLoadMore{
  cursor:hand; cursor:pointer;
  text-align:center;
  background: linear-gradient(#9D3B6E, #91235C);
  background-color:#9D3B6E;
  padding:10px 0px 10px 0px;
  border-radius:4px;
  color:#FFF;
  font-size:18px;
  margin-top:10px;
}

.bidValue{
    width:100%; max-width:100%;
    padding:8px 10px;
    font:inherit;
    color:gray;
    border: 1px solid rgb(218, 218, 218);
    border-radius:3px;
    background-color: #F2F2F2;
}