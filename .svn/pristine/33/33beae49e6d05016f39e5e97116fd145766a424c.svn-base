      div.topmenu .categorybutton a span
      {
        float: right;
        color: #fff;
        text-align: center;
        font-size: 1.4rem;
        font-weight: 900;
        -webkit-text-shadow: 2px 2px 2px #000000;
        text-shadow: 2px 2px 2px #000000;
        font-family: Arial, Helvetica, sans-serif,;
      }

    div.topmenu .categorybutton button
    {
      float: right;
      padding: 7px 12px;
      background-color: #272a28;
      background-image: -webkit-linear-gradient(bottom, #202020, #3e3e3e 57%, #393939);
      background-image: -moz-linear-gradient(bottom, #202020, #3e3e3e 57%, #393939);
      background-image: -o-linear-gradient(bottom, #202020, #3e3e3e 57%, #393939);
      background-image: -ms-linear-gradient(bottom, #202020, #3e3e3e 57%, #393939);
      background-image: linear-gradient(to top, #202020, #3e3e3e 57%, #393939);
      background-image: -webkit-gradient(linear, center bottom, center top, from(#202020), color-stop(57%, #3e3e3e), to(#393939));
      border: solid 2px #000;
      -webkit-border-radius: 9px/9px 9px 8px 8px;
      -moz-border-radius: 9px/9px 9px 8px 8px;
      border-radius: 9px/9px 9px 8px 8px;
      -webkit-box-shadow: 0 1px 3px rgba(255,255,255,.22), inset 0 4px 5px rgba(23,23,23,.61);
      -moz-box-shadow: 0 1px 3px rgba(255,255,255,.22), inset 0 4px 5px rgba(23,23,23,.61);
      box-shadow: 0 1px 3px rgba(255,255,255,.22), inset 0 4px 5px rgba(23,23,23,.61);
    }

      div.topmenu .categorybutton button span
      {
        float: right;
        color: #fff;
        text-align: center;
        font-size: 1.6rem;
        font-weight: bold;
        -webkit-text-shadow: 2px 2px 2px #000000;
        text-shadow: 2px 2px 2px #000000;
        font-family: Verdana;
      }

