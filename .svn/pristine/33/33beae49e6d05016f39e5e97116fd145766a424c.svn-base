/*Content list*/
div.videoitem
{
  border-top: 2px solid #414040;
  border-bottom: 1px solid #000000;
  background: #343434; /* Old browsers */
  background: -moz-linear-gradient(top, #343434 0%, #343434 56%, #2f2f2f 76%, #292929 88%, #282829 100%); /* FF3.6+ */
  background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#343434), color-stop(56%,#343434), color-stop(76%,#2f2f2f), color-stop(88%,#292929), color-stop(100%,#282829)); /* Chrome,Safari4+ */
  background: -webkit-linear-gradient(top, #343434 0%,#343434 56%,#2f2f2f 76%,#292929 88%,#282829 100%); /* Chrome10+,Safari5.1+ */
  background: -o-linear-gradient(top, #343434 0%,#343434 56%,#2f2f2f 76%,#292929 88%,#282829 100%); /* Opera 11.10+ */
  background: -ms-linear-gradient(top, #343434 0%,#343434 56%,#2f2f2f 76%,#292929 88%,#282829 100%); /* IE10+ */
  background: linear-gradient(to bottom, #343434 0%,#343434 56%,#2f2f2f 76%,#292929 88%,#282829 100%); /* W3C */
  filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#343434', endColorstr='#282829',GradientType=0 ); /* IE6-9 */
}

  div.videoitem .thumbnail
  {
    min-width: 165px;
    display: block;
  }

    div.videoitem .thumbnail a.thumbnailimg
    {
      margin-bottom: -3px;
      width: 51.5625%;
      position: relative;
      display: block;
    }

    div.videoitem .thumbnail img
    {
      width: 100%;
    }

      div.videoitem .thumbnail img.overlay
      {
        position: absolute;
        left: 0;
        top: 0;
      }


  div.videoitem .description
  {
    display: block;
    width: 47.4375%;
    padding-left: 1%;
  }

    div.videoitem .description a.titlelink
    {
      display: table-cell !important;
    }

    div.videoitem .description strong.title
    {
      color: #FFFFFF;
      font-size: 1.4rem;
      font-weight: normal;
      font-family: Tahoma,sans-serif;
      -webkit-text-shadow: 1px 2px #000000;
      text-shadow: 1px 2px #000000;
      white-space: nowrap;
    }

    div.videoitem .description .rating
    {
      margin: 2px 0 2px;
      background-image: url(/Images/base/stars/fsgn.png);
      background-repeat: no-repeat;
      -moz-background-size: 94px 16px;
      -o-background-size: 94px 16px;
      -webkit-background-size: 94px 16px;
      background-size: 94px 16px;
      height: 16px;
      width: 94px;
    }

      div.videoitem .description .rating img
      {
        height: 16px;
        width: 94px;
        position: absolute;
      }

    div.videoitem .description strong.duration
    {
      color: #B2B1B1;
      font-size: 1.2rem;
      font-family: Tahoma,sans-serif;
      font-weight: normal;
    }

div.watch
{
  margin: 10px 0 0;
  width: 90%;
}

  div.watch a.watchnow
  {
    width: 100%;
    min-width: 70px;
    max-width: 375px;
    color: #FFFFFF;
    padding: 9px 20px;
    background: -moz-linear-gradient(top, #ffcc00 0%, #ffaa00);
    background: -webkit-gradient(linear, left top, left bottom, from(#ffcc00), to(#ffaa00));
    -moz-border-radius: 5px;
    -webkit-border-radius: 5px;
    border-radius: 5px;
    border: 1px solid #000000;
    -moz-box-shadow: 1px 1px 2px rgba(000,000,000,0.5), inset 0px 1px 0px rgba(255,255,255,0.9);
    -webkit-box-shadow: 1px 1px 2px rgba(000,000,000,0.5), inset 0px 1px 0px rgba(255,255,255,0.9);
    box-shadow: 1px 1px 2px rgba(000,000,000,0.5), inset 0px 1px 0px rgba(255,255,255,0.9);
    -webkit-text-shadow: 1px 0px 1px rgba(000,000,000,0.3), 1px 1px 0px rgba(255,255,255,0);
    text-shadow: 1px 0px 1px rgba(000,000,000,0.3), 1px 1px 0px rgba(255,255,255,0);
    text-align: center;
    font-size: 1.3rem;
    font-family: Arial, Helvetica, sans-serif;
  }

    div.watch a.watchnow span
    {
      color: #FFFFFF;
      font-weight: bold;
      -webkit-text-shadow: 1px 1px 2px #000000;
      text-shadow: 1px 1px 2px #000000;
      filter: dropshadow(color=#000000, offx=1, offy=1);
    }


