.disc a {
    font-weight: 100;
    list-style-type: none;
    color: #333
}


.disc {
    /* margin-top:50px; */
    
    width: 100%;
    margin: auto;
    font-size: 11px;
    /* padding-top:30px; */
    
    overflow: hidden;
    background-color: black;
    width: 60%!important;
    text-align: left;
    margin-top: 20px;
}
li {
    display: inline-block
}
.specs {
    margin-top: 0;
    width: 65%
}
.phoneNumber {
    height: 42px;
    padding: 0 0 0 8px;
    font-size: 28px;
    line-height: 1.42857143;
    color: #555;
    margin-top: 19px;
    width: 90%;
    border: 1px solid #ccc;
    border-radius: 4px;
    box-shadow: inset 0 1px 1px rgba(0, 0, 0, .075);
    -webkit-transition: border-color ease-in-out .15s;
    -webkit-box-shadow: ease-in-out .15s;
    -o-transition: border-color ease-in-out .15s, box-shadow ease-in-out .15s;
    transition: border-color ease-in-out .15s, box-shadow ease-in-out .15s
}
#msisdn {
    font-size: 24px;
    width: 320px;
    padding-left: 4px 5px 4px 10px;
    border: 2px solid #e5e5e5;
    margin-top: -4px;
    margin-bottom: 10px
}
#error_number {
    display: none
}
.error {
    margin-bottom: 5px
}
.button {
    -moz-box-shadow: inset 0 1px 0 0 #54a3f7;
    -webkit-box-shadow: inset 0 1px 0 0 #54a3f7;
    box-shadow: inset 0 1px 0 0 #54a3f7;
    background: -webkit-gradient(linear, left top, left bottom, color-stop(.05, #007dc1), color-stop(1, #0061a7));
    background: -moz-linear-gradient(top, #007dc1 5%, #0061a7 100%);
    background: -webkit-linear-gradient(top, #007dc1 5%, #0061a7 100%);
    background: -o-linear-gradient(top, #007dc1 5%, #0061a7 100%);
    background: -ms-linear-gradient(top, #007dc1 5%, #0061a7 100%);
    background: linear-gradient(to bottom, #007dc1 5%, #0061a7 100%);
    filter: progid: DXImageTransform.Microsoft.gradient(startColorstr='#007dc1', endColorstr='#0061a7', GradientType=0);
    background-color: #007dc1;
    -moz-border-radius: 3px;
    -webkit-border-radius: 3px;
    border-radius: 3px;
    border: 1px solid #124d77;
    height: 50px;
    margin-top: 9px;
    margin-bottom: 3px;
    display: inline-block;
    width: 92%;
    cursor: pointer;
    font-size: 22px;
    color: #fff;
    font-family: Arial;
    text-decoration: none;
    text-shadow: 0 1px 0 #154682;
    padding: 0
}
#BTNload,
.wap {
    display: none
}
#BTNload {
    margin: 9px auto 3px
}
.tc {
    display: block;
    font-size: 12px
}
.sms {
    margin-bottom: 0
}
.koptekst {
    font-size: 10px;
    padding: 5px 0
}
.error {
    background: #FFF;
    color: #FF0004;
    font-size: 12px;
    border: 1px solid #FF0004
}
.errorTC {
    background-color: rgba(255, 255, 255, .6);
    padding: 5%;
    font-size: 12px;
    color: red;
    display: none;
    position: fixed;
    width: 90%;
    height: 100%;
    top: 0;
    left: 0;
    z-index: 100
}
#error_tc {
    -webkit-border-radius: 4px;
    -moz-border-radius: 4px;
    border-radius: 4px;
    border: 1px solid #4C4C4C;
    width: 90%;
    margin-top: 15%;
    height: 30%;
    background-color: rgba(255, 255, 255, .85);
    font-size: 1rem;
    padding: 5% 5% 0
}
#error_ok {
    height: 40%;
    width: 100%;
    font-size: 1rem;
    -moz-box-shadow: inset 0 1px 0 0 #9acc85;
    -webkit-box-shadow: inset 0 1px 0 0 #9acc85;
    box-shadow: inset 0 1px 0 0 #9acc85;
    background: -webkit-gradient(linear, left top, left bottom, color-stop(.05, #74ad5a), color-stop(1, #68a54b));
    background: -moz-linear-gradient(top, #74ad5a 5%, #68a54b 100%);
    background: -webkit-linear-gradient(top, #74ad5a 5%, #68a54b 100%);
    background: -o-linear-gradient(top, #74ad5a 5%, #68a54b 100%);
    background: -ms-linear-gradient(top, #74ad5a 5%, #68a54b 100%);
    background: linear-gradient(to bottom, #74ad5a 5%, #68a54b 100%);
    filter: progid: DXImageTransform.Microsoft.gradient(startColorstr='#74ad5a', endColorstr='#68a54b', GradientType=0);
    background-color: #74ad5a;
    border: 1px solid #3b6e22;
    color: #fff;
    cursor: pointer
}
.legal {
    font-size: 11px;
    color: #333
}
.left-container {
    width: 50%;
    float: left
}
.right-container {
    width: 50%;
    float: right
}
.product-logo {
    padding: 10px 0
}
.ans {
    font-size: 18px;
    color: #fff;
    display: inline-block
}
.mob {
    display: none
}
.toptext {
    margin-top: 40px
}
.toptext h1 {
    color: #fff;
    margin: 0;
    font-size: 50px;
    text-shadow: 1px 1px 1px rgba(0, 0, 0, .7)
}
.colorsms {
    color: #F9FF00
}
.cssModifier {
    display: none;
}
@media screen and (max-width: 1200px) {
    .product {
        width: 90%;
        margin: 0 auto
    }
    .product-logo {
        width: 95%;
        margin: 0 auto
    }
    .toptext h1 {
        font-size: 42px
    }
    .wrapper {
        padding-top: 35px
    }
    .question {
        font-size: 20px
    }
    .onderq {
        font-size: 25px
    }
    .answer {
        min-width: 120px;
        height: 120px
    }
    #answera,
    #answerb {
        background-size: 65%
    }
    .disc {
        width: 90%;
        margin: 0 auto;
        float: none;
        overflow: hidden;
        /* padding:20px 0 */
    }
}
@media only screen and (max-width: 850px) {
    .ans {
        color: #000;
        font-size: 14px
    }
    .portrait {
        height: auto
    }
    .left-container,
    .right-container {
        width: 100%;
        float: none
    }
    .desk {
        display: none
    }
    .mob {
        display: block
    }
    .product {
        width: 95%
    }
    .wrapper {
        padding-top: 20px
    }
    .specs {
        width: 90%;
        margin: 2% auto;
        float: none
    }
    .disc,
    .panel {
        width: 100%
    }
    #answera,
    #answerb {
        background-size: 65%;
        border: 2px solid #222
    }
    .colorsms {
        color: #FF9B00;
        -moz-animation: smsC 3s linear infinite;
        -webkit-animation: smsC 3s linear infinite;
        -o-animation: smsC 3s linear infinite
    }
    @keyframes smsC {
        0%, 100% {
            color: #FF9B00
        }
        50% {
            color: #B75600
        }
    }
    @-moz-keyframes smsC {
        0%, 100% {
            color: #FF9B00
        }
        50% {
            color: #B75600
        }
    }
    @-webkit-keyframes smsC {
        0%, 100% {
            color: #FF9B00
        }
        50% {
            color: #B75600
        }
    }
    .panel {
        border: 1px solid #C1C1C1;
        text-align: center;
        max-width: 95%;
        margin: 0 auto auto;
        -webkit-border-radius: 16px;
        -moz-border-radius: 16px;
        background-color: rgba(255, 255, 255, .35);
        border-radius: 5px
    }
    .ad,
    .product-logo,
    .toptext,
    .toptext2 {
        display: none
    }
    .onderq {
        font-size: 18px
    }
    .tagline {
        font-size: 15px
    }
    #error_tc {
        margin-top: 69%
    }
    .question {
        font-size: 17px
    }
}
@media screen and (max-width: 500px) {
    .onderq,
    .onderq_end {
        font-size: 22px
    }
}
@media screen and (max-width: 400px) {
    .ans {
        width: 50%;
        margin: 0 auto;
        float: left
    }
    form {
        width: 100%;
        display: inline-block
    }
}
.content-footer p b,
.title-footer {
    display: inline-block;
    text-indent: 4px
}
.title-footer {
    width: 100%;
    color: #fff;
    background: #000;
    line-height: 100%;
    border-top-right-radius: 20px;
    border-bottom-right-radius: 20px;
    padding: 2px;
    padding-right: 15px;
    /* position: absolute; */
    
    left: 0px;
    top: 0px;
    z-index: 50;
}
.content-footer p {
    display: inline-block;
    width: 100%;
    padding: 2px;
    margin: 0
}
.content-footer span {
    position: relative;
    z-index: 1;
    width: 64%;
    display: inline-block;
    font-weight: 400;
    color: #303030;
    background: #fff;
    -webkit-border-radius: 5px;
    border-radius: 5px;
    padding: 2px 4px
}
.content-footer p b {
    width: 32%;
    color: #fff
}
.content-footer {
    position: relative;
    display: block;
    font-family: Arial, sans-serif;
    font-size: 10px;
    font-weight: 700;
    color: #474747;
    background: #383838;
    -webkit-border-radius: 20px;
    border-radius: 20px;
    /* margin:10px auto; */
    
    padding-bottom: 5px;
}
.content-footer .title-footer span {
    display: inline-block;
    text-align: center;
    width: 15px;
    height: 15px;
    font-size: 10px;
    font-weight: 700;
    -webkit-border-radius: 5px;
    border-radius: 5px;
    padding: 0;
    margin-right: 4px
}
.footerlogo {
    position: absolute;
    z-index: 0;
    display: block;
    top: 10px;
    right: 5px;
    float: right
}
.footerlogo img {
    width: 28px;
    height: 28px
}
@media screen and (min-width: 480px) {
    .content-footer p,
    .content-footer p b,
    .content-footer span,
    .title-footer {
        display: inline-block;
        width: auto
    }
    .content-footer p {
        white-space: nowrap
    }
    .footerlogo,
    .footerlogo img {
        width: 50px;
        height: 50px;
        top: 0;
        right: 0
    }
    .info {
        font-weight: 700;
        -webkit-border-radius: 50px;
        border-radius: 50px;
        background-color: red;
        width: 12px;
        color: #fff;
        font-size: 14px;
        padding: 2px;
        display: inline-block;
        margin-right: 5px
    }
}
@media screen and (max-width: 450px) {
    .title-footer {
        padding: 5px;
        width: auto
    }
    .footerlogo {
        top: 1px
    }
}
.disc {
    margin-top: 20px!important;
}
@media screen and (max-width: 900px) {
    .disc p {
        width: 100%;
    }
    .disc p b {
        width: 16%!important;
    }
    .disc {
        width: 100%!important;
    }
    .cssModifier {
        display: initial;
    }
    .disc p span {
        width: auto!important;
    }
}