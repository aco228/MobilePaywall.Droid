  bool isMembershipEnabled = false;

  string displayLogo = "block";
  string logo = "/Images/_Logo/" + string.Format("{0}.png", PaywallHttpContext.Current.Service.ServiceData.ID);
  if (!System.IO.File.Exists(Server.MapPath(logo)))
