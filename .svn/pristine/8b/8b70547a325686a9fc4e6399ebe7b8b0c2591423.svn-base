function call(number) {
  window.open('tel:+' + number);
  $('#info').css('display', 'none');
  $('#callwait').css('display', 'block');
}

function wifi(obj) {
  var json = $.parseJSON(obj.responseText);
  if (json.result == true) {
    window.location.replace(json.redirect);
    return false;
  } else {
    window.location.replace(json.redirect);
    return false;
  }
}