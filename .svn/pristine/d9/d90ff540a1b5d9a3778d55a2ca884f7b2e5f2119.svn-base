.filterBypassTable {
    border: 1px solid rgba(25, 118, 210, 0.13);
    border-collapse: inherit;
    padding-top: 0px;
    padding-left: 5px;
    padding-bottom: 0px;
    padding-right: 5px;
    background-color: #f5fff5;
    border-radius: 15px;
    width: 100%;
  }

    .filterBypassTable td {
      border-bottom: 1px solid #e0e0e0;
      height: 35px;
    }

    .filterBypassTable tr:last-child td {
      border-bottom: none;
    }

  .filterName {
    font-weight: bold;
    font-size: 16px;
    padding-left: 5px;
  }

  .notEnabled {
    background-color: #ffe7e7;
  }

  .btnDisabled {
    background-color: #b1abab;
    color: white;
    pointer-events: none;
    opacity: 0.6;
    cursor: not-allowed;
  }

  .specific {
    font-size: 10px;
  }