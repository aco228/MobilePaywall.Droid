/* Fix footer page */
.page-wrap
{
    min-height: 100%;
    /* equal to footer height */
    position: relative;
}



/* INDEX */
.container
{
    width: 420px;
    margin: 0 auto;
}
.row
{
    width: 100%;
    float: left;
}
.head img
{
    display: block;
    width: 100%;
    max-width: 420px;
    float: left;
    margin: 0 auto;
    height: auto;
}
.head .title
{
    color: #666666;
    float: left;
    font-size: 14px;
    font-weight: normal;
    line-height: 1;
    padding: 5px 0;
    text-align: center;
    text-decoration: none;
    width: 100%;
}
.imb
{
    float: left;
    width: 80px;
    height: 80px;
    margin: 0 5px 0 0;
    cursor: pointer;
    position: relative;
    border-radius: 10px;
    overflow: hidden;
}
.imb:first-of-type
{
    margin: 0 5px 0 86px;
}
.icons-m
{
    float: left;
    width: 80px;
    height: 80px;
}
.icons-m.one
{
    background: url("../img/icons/BT_T.png") no-repeat center center transparent;
}
.icons-m.two
{
    background: url("../img/icons/BT_3.png") no-repeat center center transparent;
}
.icons-m.three
{
    background: url("../img/icons/BT_A1.png") no-repeat center center transparent;
}
.na
{
    width: 80px;
    height: 80px;
    position: absolute;
    top: 0;
    left: 0;
    right: 0;
    background: rgba(0,0,0,0.8);
    z-index: 1;
}
.na.edit
{
    width: 100%;
    height: 54px;
    position: absolute;
    top: 15px;
    left: 0;
    right: 0;
    background: rgba(0,0,0,0.8);
    z-index: 1;
}
.fbox
{
    position: relative;
    width: 100%;
    float: left;
}
.form-control
{
    background-color: #ffffff;
    border: 2px solid #999999;
    color: #000000;
    font-size: 24px;
    height: 30px;
    margin: 15px 0 5px;
    padding: 10px;
    width: 396px;

    transition: all 0.30s linear 0s;
    -moz-transition: all 0.30s linear 0s;
    -webkit-transition: all 0.30s linear 0s;
    -o-transition: all 0.30s linear 0s;
}
.form-control:focus,
.form-control.active
{
    border-color: #ffcb0a;
    color: #666666;
}
.infot
{
    color: #ffffff;
    float: left;
    font-size: 10px;
    font-weight: normal;
    line-height: 1;
    text-align: center;
    text-decoration: none;
    width: 100%;
    margin: 0 0 5px;
}
.infot.end
{
    margin: 10px 0 60px;
}
.line-h
{
    width: 100%;
    float: left;
    height: 1px;
    background: #666666;
}
.f-menu
{
    width: 100%;
    float: left;
    text-align: center;
    margin: 0 0 15px;
}
.f-menu li a
{
    color: #000000;
    float: left;
    font-size: 10px;
    font-weight: normal;
    line-height: 14px;
    padding: 5px 2px;
    text-decoration: none;
}
.f-menu li a:hover
{
    color: #AF96C3;
}




