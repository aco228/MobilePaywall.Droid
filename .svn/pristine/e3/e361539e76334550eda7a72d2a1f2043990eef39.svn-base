body {
  margin:0px!important; padding:0px!important;
  background-color:rgb(29,29,29)!important;
  margin:0px!important;
  color: #DADADA!important;
  /*font-family: 'Ubuntu Condensed', sans-serif!important;*/
  font-family: 'Dosis', sans-serif!important;
}
/*#wrapper{
  width:100%;
  min-height:100%;
  position:relative;
}*/

/*
  ======================================================
  CONSTANTS

*/
.header{
  color:#FFF;
  font-size:20px;
  padding-top:3px; padding-bottom:3px;
  margin-top:15px; margin-bottom:15px;
  text-transform:uppercase;
  border-bottom: 5px solid rgb(23,23,23);
  font-weight:100;
}
.header1{ font-size:17px; border:none; }
a{
  color:inherit!important;
  text-decoration:inherit!important;
}
a:hover, button:hover {
  color:inherit!important;
  background-color:inherit!important;
}

.btn:hover{color:inherit!important;}
/*
  ======================================================
  CONTENT

*/

#page_content{
  padding:10px;
  overflow:hidden;
}
#page_content a{
  text-decoration:underline!important;
}

.aditionalText{
  margin:0px;
  background-color:rgba(0,0,0,.2);
  padding:7px 0px;
  margin-top:15px;
  font-size:14px; 
  text-align:center;
}
.btn{
  background: linear-gradient(rgb(74,74,74), rgb(61,61,61));
  background-color:rgb(74,74,74);
  border: 1px solid rgba(0,0,0,.5);;
  padding:10px 30px;
  color:#FFF;
  font:inherit;
  font-size:14px;
  cursor:hand; cursor:pointer;
  text-shadow:1px 1px 1px rgba(0,0,0,.5);
  border-radius:5px;
  white-space: initial!important;
}

input[type=submit], .submitBtn{
  font:inherit;
  text-align:center;
  background-color:rgb(255, 86, 172);;
  color:#FFF;
  width:100%;
  margin-top:15px;
  padding:13px 0px;
  border-radius:5px;
  border:none;
  background:linear-gradient(rgba(255,89,172,.8), rgb(255,89,172));
  text-transform:uppercase;
  font-weight:bold;
  border:1px solid rgb(255,89,172);
  cursor:hand; cursor:pointer;
}

.button, #controlLoadMore{
  cursor:hand; cursor:pointer;
  text-align:center;
  background-color:rgb(60,60,60);
  padding:10px 0px 10px 0px;
  border-radius:4px;
  color:#FFF;
  font-size:18px;
  margin-top:10px;
}

/* Auction specific */
.__auction_customerHeader { background-color:red; padding:5px;  margin:5px;margin-bottom:-10px; }

/* quiz */
#quizQuestionData, #quizQuestionDataOLD { transition:all .4s; }
#quizQuestionWait { transition:all .4s; }
#quizTimer {
  background-color: #3db649;
  width:150px;
  text-align:center;
  color: white;
  border-radius:9px;
  font-size:18px; padding:5px 10px;
  background: linear-gradient(to right, #88c53f, #3fb649);
  text-shadow:0px 0px 2px rgba(0,0,0,.5);
}
.quiz_translation_percent, .quiz_translation_timer {
  background-color: #545454;
  padding: 2px 10px;
  border-radius:9px;
}
#quiz_finalTranslation {
  font-size:20px; text-align:center;
  margin-bottom:50px;
}
.quizAnswer{
  transition:all .5s;
  padding:5px;
  background-color: black;
  margin:5px;
  cursor:hand;
  cursor: pointer;
}
.quizSubmitNotActive{pointer-event:none;opacity: .8;}

.quizAnswerClicked{background-color: #adadad; color:black; }
.quizAnswerCorect{background-color: #1d651d;}
.quizAnswerWrong{background-color: #883939;}