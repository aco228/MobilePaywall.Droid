/*////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////*/
/*//////////////////// Green Skin CSS Rules Start                                                                         */
/*////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////*/
a, p > a {
	color: #65c178;
}
.headerWrapper {
	border-color: #65c178;
}
.mainMenuWrapper {
	border-color: #65c178;
}
.mainMenuWrapper > li > a {
	background-image: url(../../../Images/Crikey/skins/green/mainMenuItemBg.png);
}
.mainMenuWrapper > li.currentPage > a {
	color: #65c178;
}
.nivo-directionNav a {
	background-color: #65c178;
}
.recentProjectsTitle {
	border-color: #65c178;
}
.currentPageNumber, .currentPageNumber:hover {
	background-color: #65c178;
}
input.errorSearchFormSubmitButton, input.errorSearchFormSubmitButton:hover {
	background-color: #65c178;
}
.portfolioOneItemImageWrapper, .portfolioOneFilterableWrapper .portfolioFilterableItemImageWrapper {
	border-color: #65c178;
}
.portfolioMenuWrapper .currentPortfolioFilter {
	color: #65c178;
}
.portfolioOneExpandButton, .portfolioFilterableExpandButton, .singleProjectExpandButton {
	background-image: url(../../../Images/Crikey/skins/green/portfolioExpandButtonBg.png);
}
.portfolioOneDetailsButton, .portfolioFilterableDetailsButton, .singleProjectDetailsButton {
	background-image: url(../../../Images/Crikey/skins/green/portfolioDetailsButtonBg.png);
}
.postThumbnailWrapper {
	border-color: #65c178;
}
.smallPostMoreButton {
	background-image: url(../../../Images/Crikey/skins/green/smallPostReadMoreButtonBg.png);
}
input.contactSubmitButton, input.contactSubmitButton:hover {
	background-color: #65c178;
}
input.commentSubmitButton, input.commentSubmitButton:hover {
	background-color: #65c178;
}
.accordionButton .accordionButtonIcon {
	background-image: url(../../../Images/Crikey/skins/green/accordionButtonBg.png);
}
.footerWrapper {
	background-color: #65c178;
}
@media (-webkit-min-device-pixel-ratio: 2), only screen and (min--moz-device-pixel-ratio: 2), only screen and (-o-min-device-pixel-ratio: 2) {
.mainMenuWrapper > li > a {
	background-image: url(../../../Images/Crikey/skins/green/mainMenuItemBg-x2.png);
}
.portfolioOneExpandButton, .portfolioFilterableExpandButton, .singleProjectExpandButton {
	background-image: url(../../../Images/Crikey/skins/green/portfolioExpandButtonBg-x2.png);
}
.portfolioOneDetailsButton, .portfolioFilterableDetailsButton, .singleProjectDetailsButton {
	background-image: url(../../../Images/Crikey/skins/green/portfolioDetailsButtonBg-x2.png);
}
.smallPostMoreButton {
	background-image: url(../../../Images/Crikey/skins/green/smallPostReadMoreButtonBg-x2.png);
}
.accordionButton .accordionButtonIcon {
	background-image: url(../../../Images/Crikey/skins/green/accordionButtonBg-x2.png);
}
}
/*////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////*/
/*//////////////////// Green Skin CSS Rules End                                                                           */
/*////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////*/
