.thumbs {
    padding-top: 3px;
    float: left;
    margin: 20px;
  }

  .thumb_holder {
    position: relative;
    height: 250px;
    width: 250px;
    margin: 5px;
    float: left;
  }

  .thumb_image {
    position: absolute;
    z-index: 50;
    top: 0px;
    left: 0px;
    width: 100%;
    height: 100%;
    background-position: center;
    background-size: cover;
    background-repeat: no-repeat;
  }

  .thumb_overlay {
    position: absolute;
    z-index: 150;
    top: 0px;
    left: 0px;
    width: 100%;
    height: 100%;
    background-size: 30px;
  }

  .thumb_overlay_default {
    background-image: url("../../Images/Template/ContentEditor/success.png");
    background-repeat: no-repeat;
    background-position: bottom right;
  }

  .thumb_overlay:hover {
    border: 2px solid blue;
  }

  .thumb_selected {
    border: 2px solid red;
    background-color: rgba(255,250,250,0.3);
  }

  .content_detail_table {
    border: 1px solid rgba(25, 118, 210, 0.13);
    border-collapse: inherit;
    padding-top: 5px;
    padding-left: 5px;
    padding-bottom: 10px;
    padding-right: 5px;
    background-color: #f7f7f7;
    border-radius: 15px;
  }

  table.content_detail_table tr:last-child{

  }

  .content_detail_wraper {
    margin: 20px;
    margin-bottom: 50px;
  }

  tr:last-child td{
    border-bottom:none;
  }
