@model MobilePaywall.Web.Models.NotificationViewModel
@{
  Layout = string.Format("~/Views/{0}/_Layout/_Layout.cshtml", PaywallHttpContext.Current.Service.ServiceData.Template.Name);
}

@Styles.Render("~/Style/Pink/Error")

<div class="header">You have successfully unsubscribed!</div>

<div id="error_page">
  <div id="error_title">@Model.Message</div> <br />
  <div id="error_title">If you have any trouble canceling subscription go to <a href="http://eg1.just-fun.mobi/imprint">Imprint Page</a> and contact us.</div>
  <div id="error_message">@*@Model.Message*@</div>
</div>