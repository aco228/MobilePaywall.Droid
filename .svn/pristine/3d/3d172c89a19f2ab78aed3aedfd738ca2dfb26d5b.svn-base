      function validateCustInfo()
      {
      var x = document.getElementById("email").value;
      		var atpos = x.indexOf("@");
      		var dotpos = x.lastIndexOf(".");
      
      		
      		if (x == null || x.trim() == "" || (atpos< 1 || dotpos<atpos+2 || dotpos+2>=x.trim().length)) {
      		
        		 		document.getElementById('valid_email_bg').style.visibility="visible";
      document.getElementById('valid_email_text').style.visibility="visible";
      
         			return false;
      		} else {
      			document.getElementById('valid_email_bg').style.visibility="hidden";
      document.getElementById('valid_email_text').style.visibility="hidden";
      		}
      		
      
      		return true;
      }
      
