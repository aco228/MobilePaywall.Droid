                  { date: '01/01/2016 12:30:00 AM', volume: 0, volumeIndex: '100%', value: 0, valueIndex: '0%', value1: 0, valueIndex1: '0%', value2: 0, valueIndex2: '0%', value3: 0, valueIndex3: '0%', value4: 0, valueIndex4: '0%' }];
    split = "mm";

