html, body {
  margin: 0;
  padding: 0;
  height:100%;
}

body {
  background: linear-gradient(#c600bd, #8c0085);
  overflow-x: hidden;
}

#templateHeader {
  background-image: url(../imgs/glow.png);
  border-bottom: 1px solid rgba(255,255,255,0.40);
  background-position: 100% 50%;
  margin-bottom: 25px;
  border-bottom: none;
}

#header {
  background: transparent;
  width: 100%;
  height: 70px;
  padding-top: 15px;
}

#logo {
  /* padding-top:15px; */
  width: 100%;
  height: 60px;
  /* background-image: url(../imgs/glow.png); */
  background-position: 100% 50%;
  margin-left: auto;
  margin-right: auto;
  display: block;
}

  #logo img {
    width: 200px;
    margin-left: auto;
    margin-right: auto;
    display: block;
  }

.infotext {
  text-align: center;
  text-transform: uppercase;
  font-family: Segoe, "Segoe UI", "DejaVu Sans", "Trebuchet MS", Verdana, sans-serif;
  font-size: 10px;
  color: #ffffff;
  letter-spacing: 1px;
  font-weight: bold;
  padding: 7px 0px;
}

#infotext_footer {
  padding-top: 10px;
}

#subheader {
  background: #4e004d;
  text-align: center;
  text-transform: uppercase;
  font-family: Segoe, "Segoe UI", "DejaVu Sans", "Trebuchet MS", Verdana, sans-serif;
  font-size: 16px;
  letter-spacing: 20px;
  padding: 10px 0;
  color: #ffafc7;
  width: 100%;
}

#videoswrapper {
  width: 100%;
}

  #videoswrapper ul {
    padding: 0;
    margin: 0;
  }

  #videoswrapper li {
    background: linear-gradient(#8c0085, #60005b)!important;
    list-style-type: none;
    width: 100%;
    background-image: -webkit-linear-gradient(270deg,rgba(234,136,166,1.00) 0%,rgba(180,53,91,1.00) 100%);
    background-image: -moz-linear-gradient(270deg,rgba(234,136,166,1.00) 0%,rgba(180,53,91,1.00) 100%);
    background-image: -o-linear-gradient(270deg,rgba(234,136,166,1.00) 0%,rgba(180,53,91,1.00) 100%);
    background-image: linear-gradient(180deg,rgba(234,136,166,1.00) 0%,rgba(180,53,91,1.00) 100%);
    height: 91px;
    padding: 10px 0;
  }

    #videoswrapper li a {
      margin: 0px;
      display: block;
      width: 100%;
      height: 100%;
    }

.column {
  /* width: 400px; */
  margin-left: auto;
  margin-right: auto;
}

.left {
  position: relative;
  width: 120px;
  height: 70px;
  border: medium solid rgba(255,255,255,1.00);
  float: left;
  margin-left: 10px;
}

.contentIcon {
  position: absolute;
  top: 0px;
  left: 0px;
  width: 100%;
  height: 100%;
  background-image: url(../imgs/Play.png);
  background-size: 20px;
  background-repeat: no-repeat;
  background-position: center;
  z-index: 500;
}

.contentThumb {
  position: absolute;
  top: 0px;
  left: 0px;
  width: 100%;
  height: 100%;
  background-image: url(http://cdn1.umg3.net/89/files/2014/06/Album-image.png);
  background-size: cover;
  background-repeat: no-repeat;
  background-position: center;
}

.playoverlay img {
  width: 20px;
  height: auto;
  display: block;
  margin-left: auto;
  margin-right: auto;
  margin-top: 27px;
  opacity: 0.8;
}

.playoverlaylarge img {
  width: 30px;
  height: auto;
  display: block;
  margin-left: auto;
  margin-right: auto;
  margin-top: 85px;
  opacity: 0.8;
}

.right {
  float: left;
  margin-left: 15px;
}




@media (max-width:420px) {
   .title {
        font-size:15px!important;
    }
}
@media (max-width:370px) {
   .title {
        font-size:14px!important;
    }
}
@media (max-width:340px) {
   .title {
        font-size:13px!important;
    }
}

.title {
  text-align: left;
  font-family: Segoe, "Segoe UI", "DejaVu Sans", "Trebuchet MS", Verdana, sans-serif;
  font-size: 18px;
  font-weight: bold;
  color: #ffffff;
  letter-spacing: -1px;
}

.subtitle {
  text-align: left;
  font-family: Segoe, "Segoe UI", "DejaVu Sans", "Trebuchet MS", Verdana, sans-serif;
  font-size: 15px;
  font-weight: thin;
  color: #ffffff;
  letter-spacing: 2px;
}

.views {
  text-align: left;
  font-family: Segoe, "Segoe UI", "DejaVu Sans", "Trebuchet MS", Verdana, sans-serif;
  font-size: 12px;
  font-weight: bold;
  color: #ffafc7;
  letter-spacing: 1px;
  line-height: 5.5;
}

.loadmorebtn {
  margin-top: 15px;
  width: 95%;
  height: 50px;
  margin-left: auto;
  margin-right: auto;
  color: #ffffff;
  text-align: center;
  font-family: Segoe, "Segoe UI", "DejaVu Sans", "Trebuchet MS", Verdana, sans-serif;
  font-size: 17px;
  font-weight: thin;
  color: #ffffff;
  letter-spacing: 2px;
  display: block;
  cursor: pointer;
  background: #ff00f3;
  border-left: 1px solid rgba(125, 0, 172, 0.9);
  border-right: 1px solid rgba(125, 0, 172, 0.9);
  border-bottom: 1px solid rgba(125, 0, 172, 0.9);
  border-top: 1px solid rgba(125, 0, 172, 0.9);
  border-radius: 5px;
  margin-bottom: 15px;
}

a {
  color: #c12ad8;
}

#footer {
  background: #4e004d!important;
  width: 100%;
  /* height: 30px; */
  background: #91002d;
  color: #ffffff;
  text-align: center;
  font-family: Segoe, "Segoe UI", "DejaVu Sans", "Trebuchet MS", Verdana, sans-serif;
  font-size: 10px;
  text-transform: uppercase;
  padding: 15px 0px;
  font-weight: bold;
  letter-spacing: 2px;
  margin-left: auto;
  margin-right: auto;
}

#content {
  width: 95%;
  margin-left: auto;
  margin-right: auto;
  margin-top: 10px;
  margin-bottom: 10px;
  height: 200px;
  border: medium solid rgba(255,255,255,1.00);
}

#videotitle {
  width: 95%;
  margin-left: auto;
  margin-right: auto;
  height: 30px;
}

.videotitle_left {
  text-align: left;
  font-family: Segoe, "Segoe UI", "DejaVu Sans", "Trebuchet MS", Verdana, sans-serif;
  font-size: 18px;
  font-weight: bold;
  color: #ffffff;
  letter-spacing: 1px;
  float: left;
}

.videotitle_right {
  text-align: right;
  font-family: Segoe, "Segoe UI", "DejaVu Sans", "Trebuchet MS", Verdana, sans-serif;
  font-size: 12px;
  font-weight: bold;
  color: #ffafc7;
  letter-spacing: 1px;
  float: right;
  padding-top: 7px;
}

#text {
  width: 95%;
  margin-left: auto;
  margin-right: auto;
  margin-top: 20px;
  margin-bottom: 20px;
  text-align: justify;
  font-family: Segoe, "Segoe UI", "DejaVu Sans", "Trebuchet MS", Verdana, sans-serif;
  font-size: 15px;
  font-weight: thin;
  color: #ffffff;
}

@media (max-width:500px) {

  #infotext {
    font-size: 9px;
    font-weight: bold;
    padding-top: 7px;
  }

  #header {
    height: auto;
    padding-bottom: 5px;
  }

  #subheader {
    width: 105%;
  }

  .subtitle {
    letter-spacing: 0;
  }

  #videoswrapper li {
    clear: both;
  }

  .views {
    line-height: 4.5;
  }

  .loadmorebtn {
    margin-top: 15px;
  }
}

.download_option {
  color: black;
}

.download_button {
  background-color: #91002d;
}


#avsheader {
  text-align: center;
  text-transform: uppercase;
  font-family: Segoe, "Segoe UI", "DejaVu Sans", "Trebuchet MS", Verdana, sans-serif;
  font-size: 16px;
  font-weight: bold;
  background: #91002d;
  letter-spacing: 1px;
  padding: 10px 0;
  color: #ffafc7;
  width: 100%;
  margin-bottom: 35px;
}

.avsyes {
  width: 40%;
  height: 100px;
  margin-left: auto;
  margin-right: auto;
  background-color: #369A00;
  color: #ffffff;
  text-align: center;
  font-family: Segoe, "Segoe UI", "DejaVu Sans", "Trebuchet MS", Verdana, sans-serif;
  font-size: 17px;
  font-weight: thin;
  color: #ffffff;
  letter-spacing: 2px;
  display: block;
  cursor: pointer;
  border: 1px solid rgba(52,87,0,0.90);
  border-radius: 5px;
  margin-bottom: 15px;
  float: left;
  margin-left: 7%;
  margin-bottom: 35px;
}

.avsno {
  width: 40%;
  height: 100px;
  margin-left: auto;
  margin-right: auto;
  background-color: #720006;
  color: #ffffff;
  text-align: center;
  font-family: Segoe, "Segoe UI", "DejaVu Sans", "Trebuchet MS", Verdana, sans-serif;
  font-size: 17px;
  font-weight: thin;
  color: #ffffff;
  letter-spacing: 2px;
  display: block;
  cursor: pointer;
  border: 1px solid rgba(79,0,16,0.90);
  border-radius: 5px;
  margin-bottom: 35px;
}


.loginheader {
  text-align: center;
  text-transform: uppercase;
  font-family: Segoe, "Segoe UI", "DejaVu Sans", "Trebuchet MS", Verdana, sans-serif;
  font-size: 16px;
  background: #91002d;
  letter-spacing: 1px;
  padding: 10px 0;
  color: #ffafc7;
  width: 100%;
  margin-bottom: 15px;
}

#testing_error {
  width: 90%;
  margin-left: auto;
  margin-right: auto;
  padding: 10px;
  color: #FF0004;
  background-color: #FFEDED;
  text-align: center;
  margin-bottom: 17px;
}

.loginform {
  width: 90%;
  margin-left: auto;
  margin-right: auto;
  padding: 10px;
  color: #FF0004;
  background-color: #FFEDED;
  text-align: center;
  margin-bottom: 17px;
}

.form_section {
  font-weight: bold;
  font-size: 13px;
}

.form_label {
  color: #2D2D2D;
  margin-bottom: 10px;
  margin-top: 5px;
  font-family: Segoe, "Segoe UI", "DejaVu Sans", "Trebuchet MS", Verdana, sans-serif;
  letter-spacing: 1px;
}

.form_section input[type="text"], .form_section input[type="password"] {
  width: 90%;
  max-width: 90%;
  padding: 8px 10px;
  font: inherit;
  color: gray;
  border: 1px solid rgb(218, 218, 218);
  border-radius: 3px;
  background-color: #F2F2F2;
  margin-bottom: 15px;
}

.login_asterix {
  color: rgb(255, 86, 172);
}

.section_body {
  background-color: #FFF;
  padding: 5%;
  border: 1px solid rgb(236, 236, 236);
}
