body {
  margin:0px; padding:0px;
  background-color:#FFF;
  margin:0px;
  font-family: 'Ubuntu Condensed', sans-serif;
  background-image:url('../../Images/Pink/background.png');
  padding:0px!important;
}
/*#wrapper{
  width:100%;
  min-height:100%;
  position:relative;
}*/

/*
  ======================================================
  CONSTANTS

*/
.header{
  background-color:rgb(255, 86, 172);
  color:#FFF;
  font-size:20px;
  padding-top:3px; padding-bottom:3px;
  text-align:center;
  margin-top:15px; margin-bottom:15px;
  text-transform:uppercase;
  border: 1px solid rgb(255, 48, 154);
}
.header1{ font-size:17px; border:none; }
.header_notification{
  text-align:center;  font-size:15px; font-weight:bold;
}
.header_notification3{
  font-weight:100;
  text-transform:initial;
  padding:20px 0px;
  background-color: rgba(224, 0, 114, 0.41);
  margin:15px;
  border-radius:9px;
  text-shadow:1px 1px 0px rgba(0,0,0,.5);
  color:white;
}
a{
  color:inherit!important;
  text-decoration:underline;
}

.row{
  margin-left:0px!important;
  margin-right:0px!important;
}
.btn:hover{color:inherit!important;}

/*
  ======================================================
  CONTENT

*/

#page_content{
  padding:10px;
}

.content_aditionalText{
  margin:10px;
  margin-top:15px;
  font-size:14px; 
  text-align:center;
}

.btn{
  background: linear-gradient(rgba(255, 86, 172, .8), rgb(255, 86, 172));
  background-color:rgb(255, 86, 172);
  border: 1px solid rgb(255, 71, 165);
  padding:10px 30px;
  color:#FFF;
  cursor:hand; cursor:pointer;
  text-shadow:1px 1px 1px rgba(0,0,0,.5);
  border-radius:3px;
}

input[type=submit], .submitBtn{
  font:inherit;
  text-align:center;
  background-color:rgb(255, 86, 172);
  color:#FFF;
  width:100%;
  margin-top:15px;
  padding:13px 0px;
  border-radius:5px;
  border:none;
  background:linear-gradient(rgba(255,89,172,.8), rgb(255,89,172));
  text-transform:uppercase;
  font-weight:bold;
  border:1px solid rgb(255,89,172);
  cursor:hand; cursor:pointer;
}

.text_area{ font-size: 17px;}

.text_input{
    width:100%; max-width:100%;
    padding:8px 10px;
    font:inherit;
    color:gray;
    border: 1px solid rgb(218, 218, 218);
    border-radius:3px;
    background-color: #F2F2F2;
}

.__auction_customerHeader {
    background-color: rgb(255, 86, 172);
    padding: 5px;
    margin:15px;
    margin-bottom:-10px;
}


.button, #controlLoadMore{
  cursor:hand; cursor:pointer;
  text-align:center;
  background-color:rgb(60,60,60);
  padding:10px 0px 10px 0px;
  border-radius:4px;
  color:#FFF;
  font-size:18px;
  margin-top:10px;
}

.__auction_customerHeader_customerName {
    color:white;
}

.__auction_customerHeader_credits {
    color:white;
}



/* quiz */
#quizQuestionData, #quizQuestionDataOLD { transition:all .4s; }
#quizQuestionWait { transition:all .4s; }
.quizAnswer{
  transition:all .5s;
  padding:5px;
  background-color: #bbbbbb;
  margin:5px;
  cursor:hand;
  cursor: pointer;
}
.quizSubmitNotActive{ pointer-event:none; opacity:.2; }

.quizAnswerClicked{background-color: #d3adc0; color:black; }
.quizAnswerCorect{background-color: #1d651d;}
.quizAnswerWrong{background-color: #883939;}