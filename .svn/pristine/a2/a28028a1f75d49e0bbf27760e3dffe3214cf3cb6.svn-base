.compare_group{
    background-color:white;
    padding:10px;
    margin-top:5px; margin-bottom:5px;
}
.cg_name{ font-size:15px; font-weight:bold;}
.cg_description{ font-size:12px;}
.cg_entries{
    margin-top:20px;
    margin-left:15px;
}

.entry{ position:relative; padding:3px;margin:5px; }
.entry_result_OK{background-color: #ccffcc;}
.entry_result_Different{background-color: #ffd7d7;}
.entry_table{ font-weight:bold; color:gray; }
.entry_actions{ position:absolute; right:5px; top:10px; }
.entry_actions input[type=button]{ width: 50px; cursor:hand; cursor:pointer; }