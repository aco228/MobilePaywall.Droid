.contentGroupLabel {
    width: 100%;
    font-weight: 400;
        font-size: 9px;
    margin-top: 0px;
    margin-bottom: 0;
}

h3.contentGroupName {
    display: inline-block;
    margin-top: -5px;
    margin-left: 10px;
    margin-bottom: 0px;
    font-size: 12px;
    line-height: 12px;
}

.removeContentGroupFromService {
    float: right;
    min-width: 15px;
    min-height: 15px;
    width: 15px;
    padding: 0;
    height: 15px;
    text-align: center;
    line-height: 15px;
    vertical-align: middle;
    position: absolute;
    top: 4px;
    right: 6px;
}

.serviceGroupsClass{
      border: 1px solid rgba(25, 118, 210, 0.13);
    border-collapse: inherit;
    padding-top: 0px;
    padding-left: 5px;
    padding-bottom: 0px;
    padding-right: 5px;
    background-color: #f7f7f7;
    border-radius: 7px;
    position: relative;
    line-height: 11px;
    margin-bottom: 3px;
}