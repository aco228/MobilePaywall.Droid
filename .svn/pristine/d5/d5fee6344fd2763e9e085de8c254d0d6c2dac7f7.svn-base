function CustomSizeManager(){
  this._init = function () {
    this.Resize();
    this.OnResize();
  };

  this.OnResize = function () {
    var self = this;
    window.onresize = function () {
      self.Resize();
    };
  }

  this.Resize = function () {
    $('._videoBox').each(function () {
      var width = $(this).width() / 1.3;
      $(this).css('height', width + 'px');
    });
  };

  this._init();
}