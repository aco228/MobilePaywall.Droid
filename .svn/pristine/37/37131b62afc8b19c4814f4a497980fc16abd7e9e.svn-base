html,body {
	margin:0;
	padding: 0;
}

body {
	  background: #004E9D;
    background: -webkit-linear-gradient(#004E9D, #01346C);
    background: -o-linear-gradient(#004E9D, #01346C);
    background: -moz-linear-gradient(#004E9D, #01346C);
    background: linear-gradient(#004E9D, #01346C);
	overflow-x: hidden;
}

#templateHeader{
	margin-bottom:0px;
}
#header {
	      width: 100%;
    padding: 5px 9px;
    background: none;
    margin-bottom: 10px;
}

#logo {
	position: inherit;
}

#logo img {
	width: 200px;
  margin: 0 auto;
    display: block;
}

.infotext {
	    text-align: center;
    text-transform: uppercase;
    font-family: Segoe, "Segoe UI", "DejaVu Sans", "Trebuchet MS", Verdana, sans-serif;
    font-size: 10px;
    color: white;
    letter-spacing: 1px;
    font-weight: bold;
    padding: 7px 0px;
}
#infotext_footer{
	padding-top:10px;
}

#subheader {
	  letter-spacing: 2px;
    font-size: 27px;
    font-family: Papyrus, fantasy;
    padding: 5px 13px;
    color: white;
    margin-bottom: 5px;
    background-image: url(../../Images/PlayLine/textbackground.png);
    background-size: contain;
    background-repeat: no-repeat;
    background-position: center center;
    width: 100%;
    height: 109px;
    text-align: center;
    padding-top: 20px;
    text-shadow: 1px 0 black, 0 2px black, 2px 0 black, 0 2px black;
}

#videoswrapper {
	width: 100%;
  padding-bottom:10px;
    color: white;
    margin-top: 30px;
}

#videoswrapper ul {
 	margin: 0;
  padding:0;
}

#videoswrapper li {
  	list-style-type: none;
  	width: 100%;
    margin-bottom: 20px;
    padding: 0 35px;
}

#videoswrapper li a {
	margin: 0px;
	display: block;
	width: 100%;
	height: 100%;
}

.column {
	 width: 100%;
	margin-left: auto;
	margin-right: auto;
  clear:both;
  overflow: hidden;
}

.left {
	width: 150px;
	height: 150px;
	float: left;
  padding:5px;
}
.contentIcon{
	background-image:url(../imgs/Play.png);
	background-size:20px;
	background-repeat:no-repeat;
	background-position:center;
	z-index:500;
}
.contentThumb{
	width:100%; height:100%;
	background-image:url(http://cdn1.umg3.net/89/files/2014/06/Album-image.png);
	background-size:cover;
	background-repeat:no-repeat;
	background-position:center;
      border-radius: 50%;
    border: solid 5px #f0f338;
    box-shadow: 0px 4px 0px #e48510;
}

.playoverlay img {
	width: 20px;
	height: auto;
	display: block;
	margin-left: auto;
	margin-right: auto;
	margin-top: 27px;
	opacity: 0.8;
}

.playoverlaylarge img {
	width: 30px;
	height: auto;
	display: block;
	margin-left: auto;
	margin-right: auto;
	margin-top: 85px;
	opacity: 0.8;
}

.right {
	overflow: hidden;
    padding-left: 50px;
}

.title {
	    text-align: center;
    font-family: Segoe, "Segoe UI", "DejaVu Sans", "Trebuchet MS", Verdana, sans-serif;
    font-size: 24px;
    color: white;
    margin-top: 13px;
    margin-bottom: 9px;
}

.subtitle {
	text-align: left;
	font-family: Segoe, "Segoe UI", "DejaVu Sans", "Trebuchet MS", Verdana, sans-serif;
	font-size: 15px;
	font-weight: thin;
	color: black;
}

.views {
	text-align: left;
	font-family: Segoe, "Segoe UI", "DejaVu Sans", "Trebuchet MS", Verdana, sans-serif;
	font-size: 12px;
	font-weight: bold;
	color: #ffafc7;
	letter-spacing: 1px;
	line-height: 5.5;
}

.loadmorebtn {
	    margin-top: 15px;
    width: 77%;
    height: 33px;
    margin-left: auto;
    margin-right: auto;
    background: #FA6905;
    text-align: center;
    font-family: Segoe, "Segoe UI", "DejaVu Sans", "Trebuchet MS", Verdana, sans-serif;
    font-size: 20px;
    color: #ffffff;
    letter-spacing: 2px;
    display: block;
    cursor: pointer;
    border-radius: 5px;
    margin-bottom: 15px;
}

a{
	color: white;
  text-decoration: none;
}

#footer {
	    width: 100%;
    background: #002C5A;
    color: #ffffff;
    text-align: center;
    font-family: Segoe, "Segoe UI", "DejaVu Sans", "Trebuchet MS", Verdana, sans-serif;
    font-size: 12px;
    text-transform: uppercase;
    padding: 15px 0px;
    font-weight: bold;
    letter-spacing: 2px;
    margin-left: auto;
    margin-right: auto;
}

#content {
	width: 95%;
	margin-left: auto;
	margin-right: auto;
	margin-top: 10px;
	margin-bottom: 10px;
	height: 200px;
	border: medium solid rgba(255,255,255,1.00);
}

#videotitle {
	width: 95%;
	margin-left: auto;
	margin-right: auto;
	height: 30px;
}

.videotitle_left {
	text-align: left;
	font-family: Segoe, "Segoe UI", "DejaVu Sans", "Trebuchet MS", Verdana, sans-serif;
	font-size: 18px;
	font-weight: bold;
	color: #ffffff;
	letter-spacing: 1px;
	float:left;
}

.videotitle_right {
	text-align: right;
	font-family: Segoe, "Segoe UI", "DejaVu Sans", "Trebuchet MS", Verdana, sans-serif;
	font-size: 12px;
	font-weight: bold;
	color: #ffafc7;
	letter-spacing: 1px;
	float: right;
	padding-top: 7px;
}

#text {
	width: 95%;
	margin-left: auto;
	margin-right: auto;
	margin-top: 0px;
	margin-bottom: 20px;
	text-align: justify;
	font-family: Segoe, "Segoe UI", "DejaVu Sans", "Trebuchet MS", Verdana, sans-serif;
	font-size: 15px;
	font-weight: thin;
}

@media (max-width:500px){

	#infotext {
	font-size: 9px;
	font-weight: bold;
	padding-top: 7px;
	}
	
	#header {
	height: auto;
	padding-bottom: 5px;
	}
	
	#subheader {
	width: 105%;
	}
		
	.subtitle {
	letter-spacing: 0;
	}
	
	#videoswrapper li {
	clear: both;
	}
	
	.views {
	line-height: 4.5;
	}
	
	.loadmorebtn {
	margin-top: 15px;
	}

}

.download_option{color:black}
.download_button{background-color: #91002d;}


#avsheader {
	text-align: center;
	text-transform: uppercase;
	font-family: Segoe, "Segoe UI", "DejaVu Sans", "Trebuchet MS", Verdana, sans-serif;
	font-size: 16px;
	font-weight: bold;
	background: #91002d;
	letter-spacing: 1px;
	padding: 10px 0;
	color: #ffafc7;
