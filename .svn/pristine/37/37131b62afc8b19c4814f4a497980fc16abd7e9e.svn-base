	margin-bottom: 15px;
}

.login_asterix {
    color: rgb(255, 86, 172);
}

.section_body {
    background-color: #FFF;
    padding: 5%;
    border: 1px solid rgb(236, 236, 236);
}


.videoPageWrapper{
      padding: 0 30px;
}

#templateContent {
    margin-top: 20px;
    color:white;
}

.playbutton {
    background-image: url(../../Images/PlayLine/playbtn.png);
