}


.loginheader {
	text-align: center;
	text-transform: uppercase;
	font-family: Segoe, "Segoe UI", "DejaVu Sans", "Trebuchet MS", Verdana, sans-serif;
	font-size: 16px;
	background: #FF9C03;
	letter-spacing: 1px;
	padding: 10px 0;
	color: white;
	width: 100%;
	margin-bottom: 15px;
}

#testing_error {
	width: 90%;
	margin-left: auto;
	margin-right: auto;
	padding: 10px;
	color: #FF0004;
	background-color: #FFEDED;
	text-align: center;
	margin-bottom: 17px;
}

.loginform {
	width: 90%;
	margin-left: auto;
	margin-right: auto;
	padding: 10px;
	color: #FF0004;
	background-color: #FFEDED;
