html,body {
	margin:0;
	padding: 0;
}

body {
	background: #FFFBD6;
	overflow-x: hidden;
}

#templateHeader{
	margin-bottom:0px;
  background-color: #FF9C03;
}
#header {
	    width: 100%;
    background: #FFFBD6;
    margin-left: 13px;
    padding: 5px 9px;
}

#logo {
	position: inherit;
}

#logo img {
	width: 200px;
}

.infotext {
	text-align: center;
    text-transform: uppercase;
    font-family: Segoe, "Segoe UI", "DejaVu Sans", "Trebuchet MS", Verdana, sans-serif;
    font-size: 10px;
    color: #FF9C03;
    letter-spacing: 1px;
    font-weight: bold;
    padding: 7px 0px;
    background-color: #FEE8B6;
}
#infotext_footer{
	padding-top:10px;
}

#subheader {
	    text-align: left;
    font-family: Segoe, "Segoe UI", "DejaVu Sans", "Trebuchet MS", Verdana, sans-serif;
    font-size: 17px;
    background: #FF9C03;
    letter-spacing: 2px;
    padding: 5px 13px;
    color: white;
    margin-bottom: 5px;
}

#videoswrapper {
	width: 100%;
  padding-bottom:10px;
    color: #FF9C03;
}

#videoswrapper ul {
  	padding: 0;
 	margin: 0;
}

#videoswrapper li {
  	list-style-type: none;
  	width: 100%;
    background-color: #FEE8B6;
    margin-bottom: 2px;
}

#videoswrapper li a {
	margin: 0px;
	display: block;
	width: 100%;
	height: 100%;
}

.column {
	/* width: 400px; */
	margin-left: auto;
	margin-right: auto;
  clear:both;
  overflow: hidden;
}

.left {
	width: 120px;
	height: 70px;
	float: left;
	margin-left: 13px;
}
.contentIcon{
	background-image:url(../imgs/Play.png);
	background-size:20px;
	background-repeat:no-repeat;
	background-position:center;
	z-index:500;
}
.contentThumb{
	width:100%; height:100%;
	background-image:url(http://cdn1.umg3.net/89/files/2014/06/Album-image.png);
	background-size:cover;
	background-repeat:no-repeat;
	background-position:center;
}

.playoverlay img {
	width: 20px;
	height: auto;
	display: block;
	margin-left: auto;
	margin-right: auto;
	margin-top: 27px;
	opacity: 0.8;
}

.playoverlaylarge img {
	width: 30px;
	height: auto;
	display: block;
	margin-left: auto;
	margin-right: auto;
	margin-top: 85px;
	opacity: 0.8;
}

.right {
	overflow: hidden;
    padding-left: 15px;
}

.title {
	text-align: left;
	font-family: Segoe, "Segoe UI", "DejaVu Sans", "Trebuchet MS", Verdana, sans-serif;
	font-size: 18px;
	color: #FF9C03;
      margin-top: 3px;
}

.subtitle {
	text-align: left;
	font-family: Segoe, "Segoe UI", "DejaVu Sans", "Trebuchet MS", Verdana, sans-serif;
	font-size: 15px;
	font-weight: thin;
	color: black;
}

.views {
	text-align: left;
	font-family: Segoe, "Segoe UI", "DejaVu Sans", "Trebuchet MS", Verdana, sans-serif;
	font-size: 12px;
	font-weight: bold;
	color: #ffafc7;
	letter-spacing: 1px;
	line-height: 5.5;
}

.loadmorebtn {
	margin-top:15px;
	width: 95%;
	height: 50px;
	margin-left: auto;
	margin-right: auto;
	background: #FA6905;
	color: #ffffff;
	text-align: center;
	font-family: Segoe, "Segoe UI", "DejaVu Sans", "Trebuchet MS", Verdana, sans-serif;
	font-size: 17px;
	font-weight: thin;
	color: #ffffff;
	letter-spacing: 2px;
	display: block;
	cursor: pointer;
	border-left: 1px solid #FA6905;
	border-right: 1px solid #FA6905;
	border-bottom: 1px solid #FA6905;
	border-top: 1px solid #FA6905;
	border-radius: 5px;
	margin-bottom: 15px;
}

a{
	color: white;
  text-decoration: none;
}

#footer {
	width: 100%;
	/* height: 30px; */
	background: #FA6905;
	color: #ffffff;
	text-align: center;
	font-family: Segoe, "Segoe UI", "DejaVu Sans", "Trebuchet MS", Verdana, sans-serif;
	font-size: 10px;
	text-transform: uppercase;
	padding: 15px 0px;
	font-weight: bold;
	letter-spacing: 2px;
	margin-left: 13px;
	margin-right: auto;
}

#content {
	width: 95%;
	margin-left: auto;
	margin-right: auto;
	margin-top: 10px;
	margin-bottom: 10px;
	height: 200px;
	border: medium solid rgba(255,255,255,1.00);
}

#videotitle {
	width: 95%;
	margin-left: auto;
	margin-right: auto;
	height: 30px;
}

.videotitle_left {
	text-align: left;
	font-family: Segoe, "Segoe UI", "DejaVu Sans", "Trebuchet MS", Verdana, sans-serif;
	font-size: 18px;
	font-weight: bold;
	color: #ffffff;
	letter-spacing: 1px;
	float:left;
}

.videotitle_right {
	text-align: right;
	font-family: Segoe, "Segoe UI", "DejaVu Sans", "Trebuchet MS", Verdana, sans-serif;
	font-size: 12px;
	font-weight: bold;
	color: #ffafc7;
	letter-spacing: 1px;
	float: right;
	padding-top: 7px;
}

#text {
	width: 95%;
	margin-left: auto;
	margin-right: auto;
	margin-top: 0px;
	margin-bottom: 20px;
	text-align: justify;
	font-family: Segoe, "Segoe UI", "DejaVu Sans", "Trebuchet MS", Verdana, sans-serif;
	font-size: 15px;
	font-weight: thin;
}

@media (max-width:500px){

	#infotext {
	font-size: 9px;
	font-weight: bold;
	padding-top: 7px;
	}
	
	#header {
	height: auto;
	padding-bottom: 5px;
	}
	
	#subheader {
	width: 105%;
	}
		
	.subtitle {
	letter-spacing: 0;
	}
	
	#videoswrapper li {
	clear: both;
	}
	
	.views {
	line-height: 4.5;
	}
	
	.loadmorebtn {
	margin-top: 15px;
	}

}

.download_option{color:black}
.download_button{background-color: #91002d;}


#avsheader {
	text-align: center;
	text-transform: uppercase;
	font-family: Segoe, "Segoe UI", "DejaVu Sans", "Trebuchet MS", Verdana, sans-serif;
	font-size: 16px;
	font-weight: bold;
	background: #91002d;
	letter-spacing: 1px;
	padding: 10px 0;
	color: #ffafc7;
	width: 100%;
	margin-bottom: 35px;
}

.avsyes {
	width: 40%;
	height: 100px;
	margin-left: auto;
	margin-right: auto;
	background-color: #369A00;
	color: #ffffff;
	text-align: center;
	font-family: Segoe, "Segoe UI", "DejaVu Sans", "Trebuchet MS", Verdana, sans-serif;
	font-size: 17px;
	font-weight: thin;
	color: #ffffff;
	letter-spacing: 2px;
	display: block;
	cursor: pointer;
	border: 1px solid rgba(52,87,0,0.90);
	border-radius: 5px;
	margin-bottom: 15px;
	float: left;
	margin-left: 7%;
	margin-bottom: 35px;
}

.avsno {
	width: 40%;
	height: 100px;
	margin-left: auto;
	margin-right: auto;
	background-color: #720006;
	color: #ffffff;
	text-align: center;
	font-family: Segoe, "Segoe UI", "DejaVu Sans", "Trebuchet MS", Verdana, sans-serif;
	font-size: 17px;
	font-weight: thin;
	color: #ffffff;
	letter-spacing: 2px;
	display: block;
	cursor: pointer;
	border: 1px solid rgba(79,0,16,0.90);
	border-radius: 5px;
	margin-bottom: 35px;
}


.loginheader {
	text-align: center;
	text-transform: uppercase;
	font-family: Segoe, "Segoe UI", "DejaVu Sans", "Trebuchet MS", Verdana, sans-serif;
	font-size: 16px;
	background: #91002d;
	letter-spacing: 1px;
	padding: 10px 0;
	color: #ffafc7;
	width: 100%;
	margin-bottom: 15px;
}

#testing_error {
	width: 90%;
	margin-left: auto;
	margin-right: auto;
	padding: 10px;
	color: #FF0004;
	background-color: #FFEDED;
	text-align: center;
	margin-bottom: 17px;
}

.loginform {
	width: 90%;
	margin-left: auto;
	margin-right: auto;
	padding: 10px;
	color: #FF0004;
	background-color: #FFEDED;
	text-align: center;
	margin-bottom: 17px;
}

.form_section {
    font-weight: bold;
    font-size: 13px;
}

.form_label {
    color: #2D2D2D;
    margin-bottom: 10px;
    margin-top: 5px;
	font-family: Segoe, "Segoe UI", "DejaVu Sans", "Trebuchet MS", Verdana, sans-serif;
	letter-spacing: 1px;
}

.form_section input[type="text"], .form_section input[type="password"] {
    width: 90%;
    max-width: 90%;
    padding: 8px 10px;
    font: inherit;
    color: gray;
    border: 1px solid rgb(218, 218, 218);
    border-radius: 3px;
    background-color: #F2F2F2;
	margin-bottom: 15px;
}

.login_asterix {
    color: rgb(255, 86, 172);
}

.section_body {
    background-color: #FFF;
    padding: 5%;
    border: 1px solid rgb(236, 236, 236);
}

#templateContent{
  background-color: #FFFBD6;
}

#templateFooter{
  background-color: #FF9C03;
}

.videoPageWrapper{
  background-color: #FEE8B6;
}