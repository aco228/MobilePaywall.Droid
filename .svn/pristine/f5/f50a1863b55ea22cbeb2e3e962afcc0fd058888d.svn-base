@model MobilePaywall.Web.Models.NotificationViewModel
@{
  Layout = string.Format("~/Views/{0}/_Layout/_Layout.cshtml", PaywallHttpContext.Current.Service.ServiceData.Template.Name);
}

@Styles.Render("~/Style/AustraliaPink/Error")

<div class="header">
  <div class="header_text">@Model.Title</div>
</div>

<div id="error_page">
  <div id="error_title">@Model.Message</div>
  <div id="error_message">@*@Model.Message*@</div>
</div>