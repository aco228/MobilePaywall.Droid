#page_header{
    margin-top:10px;
    margin-bottom:10px;
}
#header_logo{
    margin-top:5px; margin-bottom:5px;
    height:50px;
    width:100%;
    overflow:hidden;
    background-image:url('../../Images/Pink/Logo.png');
    background-size:contain;
    background-repeat:no-repeat;
    background-position:center;
}

#header_menuBtn{
    position:fixed;
    top:10px; right:0px;
    cursor:hand; cursor:pointer;
    float:right;
    background-color:#333;
    color:#FFF;
    font-size:18px;
    padding:3 10px;
    margin-right:15px;
    border-bottom:3px solid #FF55AD;
    z-index:999;
}

#header_menu_container{
    /* display:none; */
    opacity:0;
    visibility:hidden;
    position:fixed;
    top:0px; left:0px;
    width:100%; height:100%;
    z-index:998;
    background-color: rgba(51, 51, 51, 0.78);
    transition:opacity .5s linear;
    opacity:1;
    padding-top:350px;
}
.header_menuItem{
    color:#FFF;
    text-align:center;
    margin:0px 10%;
    padding:10px;
    text-shadow:0px 0px 10px rgba(0,0,0,1);
    border-bottom:1px solid #FF55AD;
}
.header_menuItem:hover{
    background-color:#FF55AD;
    cursor:hand; cursor:pointer;
}