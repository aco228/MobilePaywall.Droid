#preHrefPopupHolder {
    position:fixed;
    top:0px; left:0px;
    width:100%; height:100%;
    display: none; 
    z-index:1001;
    font-family: Arial;
    font-size: 12px;
}
#preHrefPopupHolder button{
    font-family:inherit;
    font-size:inherit;
}
.preHrefPopupHolder_back{
    position:absolute; 
    top:0px; left:0px;
    width:100%; height:100%;
    background-color:rgba(0,0,0,.7);
    z-index:-1;
    cursor:hand; cursor:pointer;
}

.preHrefPopup {
    margin-top:250px;
    position:relative;
    width:80%; margin-left:9%;
    background-color:  #696969;
    box-shadow:0 0 15px rgba(0,0,0,.7);
    border-radius:5px;
    padding:2%;
    overflow:visible;

    cursor:initial;
    color: #DADADA;
    text-shadow:1px 1px #000;
    z-index:5;
}
.preHrefPopupClose {
    position:absolute;
    right:-5px; top:-5px;
    background-color:#111111;
    color:#FFF; font-weight:bold; font-size:9px;
    padding:4px 6px; border:1px solid #696969;
    box-shadow:0px 0px 5px rgba(0,0,0,1);
    border-radius:9px;
    text-shadow:0px 0px 3px #000;
    cursor:hand; cursor:pointer;

}
.preHrefPopupSection{
    margin-top:10px;
}
.preHrefPopupAccept {
    position:relative;
    overflow:visible;
}
.preHrefPopupAlert {
    display:none; 
    position:absolute;
    top: -15%;
    height:130%; width:100%;
    box-shadow: 0px 0px 25px rgb(140, 0, 0);
    background-color:rgba(255,255,255,.2);
    pointer-events:none;
}
.preHrefPopupAccept a{
    color: rgb(176, 176, 176)!important;
    text-decoration: underline;
}
.preHrefPopupBtn {
    cursor:hand; cursor:pointer;
    width:50%; margin-left:25%;
    padding:10px;
    border-radius:8px;
    background:linear-gradient(#656364, #7E7C7D);
    border: 1px solid #585858;
    color: darkgrey;
    box-shadow: 0px 0px 15px #585858;
    font-weight:bold;
    text-shadow: 1px 1px 0px #0D0D0D;
}
.preHrefPopupAbonnement{
    text-align:center;
}
.preHrefBottomImg {
    height:93px; width:100%;
    background-image: url('../../../Images/popup/internet.png');
    background-repeat:no-repeat;
    background-position:center;
}
