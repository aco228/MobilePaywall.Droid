/*
  VIDEO OF DAY
*/
#contentVideoOfDay{}
#conten_video_ofDay{
  width:320px;
}


/*
  VIDEOS
*/
#contentVideoContainer{
  padding:10px
}
#contentVideos{
  padding-top:10px;
}
.content_video_box{
  width:100%;
  clear:both;
}

.content_video{
  float:left;
  margin:3px;

  cursor:hand; cursor:pointer;
  /*transition:all 1s;*/
}
.content_video_inside{
  position:relative;
  border:5px solid rgb(247, 3, 146);
  background-color:#333;
  /* min-height:250px; */
  
}
.content_video img{
  max-width:100%; height:auto;
}
.col-xs-6, .col-sm-6{padding:0px!important;}
.content_video_playIcon{
  position:absolute; height:100%; width:100%;
  pointer-events:none;
  background-image:url('../../Images/AustraliaPink/Play.png');
  background-size: initial;
  background-repeat:no-repeat;
  background-size: 30px;
  background-position:center;
  opacity:.8;
}
.content_video_extension{
  float:right;
  width:50%;
} 
.content_video_title{
  bottom:5px; left:5px;
  font-size:16px;
  padding:0px 15px;
  margin-bottom:5px;
  color:rgb(247, 3, 146);
}
.content_video_descritpion{
  padding:0px 15px;
  font-size:12px;
  color:rgb(247, 3, 146);
  height:30px; overflow:hidden!important;
}



#contentVideoControls{}
.content_videoControl{}
#controlLoadMore{
  cursor:hand; cursor:pointer;
  text-align:center;
  background: linear-gradient(#9D3B6E, #91235C);
  background-color:#9D3B6E;
  padding:10px 0px 10px 0px;
  border-radius:4px;
  color:#FFF;
  font-size:18px;
  margin:10px;
}