#VideoScreenshootTempObject{
    width: 100%; height: 100%;
    display: none;
}

.screenshootContainer{
    width:100%; height:100%;
}
.screenshootContainerIn{
    width:100%; height:100%;    
}
.scrshtBox{
    position:relative;
    float:left;
    overflow:hidden;
    
    background-size:cover;
    background-position:center;
}
.scrshtTxt{
    position:absolute;
    bottom:5px; right:5px;

    color:#FFF;
    background-color:rgba(0,0,0,.5);
    padding:5px 15px;
    text-shadow:2px 2px #00;
    border-radius:9px;
    pointer-events:none;
}

.scrshtCvs{
    width: 100%; height: 100%;
    pointer-events:none;
}

.scrshtLoad{
    position: absolute;
    width: 30%; height: 30%;
    margin-top:35%; margin-left: 35%;

    background-image: url('acoLoader.gif');
    background-size: 30px;
    background-repeat:no-repeat;
    background-position: center;

    background-color:rgba(255,255,255,1);
    border-radius:15px;

    box-shadow:2px 2px 12px rgba(0,0,0,.8);
    
    opacity:.4;
    pointer-events:none;
}