#cancellation_page{
   color: #cccbcb;
}

#cancellation_table_header{
    width:100%;
    text-align:center;
    font-size:13px;
    margin-bottom:5px;
}

.cancellation_table{
    margin-bottom:15px;
    width:100%;
    background-color:rgba(255,255,255,1);
    color:#000;
    font-size:13px;
    text-align:center;
    border-radius:9px;
    padding:3px;
}
.cancellation_table tr{
}
.cancellation_table td{
    border-left: 1px solid rgb(192, 192, 192);
}
.cancellation_table td:first-child{
    border-left:none
}
.cancellation_circle{
    width:25px; height:25px;
    border-radius:25px;
    margin:auto;
}
.cancellation_circle_red{ background-color:red; }
.cancellation_circle_green{ background-color:green; }

.aktiveBtn{
    padding:5px 10px;
}