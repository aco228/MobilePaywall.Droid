var chart = null;
function GenerateChart() {

    chart = AmCharts.makeChart("chartdiv", {
        "type": "stock",
        "theme": "none",
        "pathToImages": "../Scripts/metronic/global/plugins/amcharts/amstockcharts/images/",

        "categoryAxesSettings": {
            "minPeriod": split
        },
        "showOnAxis": true,
        "dataSets": [{
            "fieldMappings": [{
                "fromField": "value",
                "toField": "value"
            }, {
                "fromField": "value1",
                "toField": "value1"
            }, {
                "fromField": "value2",
                "toField": "value2"
            }, {
                "fromField": "value3",
                "toField": "value3"
            }, {
                "fromField": "value4",
                "toField": "value4"
            }, {
                "fromField": "volume",
                "toField": "volume"
            }],

            "dataProvider": chartData,
            "categoryField": "date"
        }],

        "panels": [{
            "showCategoryAxis": true,
            "title": "Value",
            "percentHeight": 70,
            //  "recalculateToPercents": "always",
            "stockGraphs": [{

                "balloonFunction": function (item) {

                    if (typeof balloonDisabled !== 'undefined' && balloonDisabled)
                        return;

                    var keys = Object.keys(item);
                    var val = item[keys[2]];

                    var val1 = val.dataContext.valueIndex;
                    var val2 = val.dataContext.value;
                    var result = val2 + "<br>" + val1;


                    return result;
                },

                "id": "g",
                "valueField": "value",
                "title": "Identifications",
                "type": "smoothedLine",
                "lineThickness": 3,
                "bullet": "round",
                "lineColor": "#723861",
                "useDataSetColors": false
            }, {
                "balloonFunction": function (item) {
                    if (typeof balloonDisabled !== 'undefined' && balloonDisabled)
                        return;
                    var keys = Object.keys(item);
                    var val = item[keys[2]];
                    var val1 = val.dataContext.valueIndex1;
                    var val2 = val.dataContext.value1;
                    var result = val2 + "<br>" + val1;
                    return result;
                },
                "id": "g1",
                "valueField": "value1",
                "title": "Payments",
                "type": "smoothedLine",
                "lineThickness": 3,
                "bullet": "round",
                "color": "#79A4A2",
                "useDataSetColors": false
            }, {
                "balloonFunction": function (item) {
                    if (typeof balloonDisabled !== 'undefined' && balloonDisabled)
                        return;
                    var keys = Object.keys(item);
                    var val = item[keys[2]];
                    var val1 = val.dataContext.valueIndex2;
                    var val2 = val.dataContext.value2;
                    var result = val2 + "<br>" + val1;
                    return result;
                },
                "id": "g2",
                "valueField": "value2",
                "title": "Transactions",
                "type": "smoothedLine",
                "lineThickness": 3,
                "bullet": "round",
                "lineColor": "#4EDF47",
                "useDataSetColors": false
            }, {
                "balloonFunction": function (item) {
                    if (typeof balloonDisabled !== 'undefined' && balloonDisabled)
                        return;
                    var keys = Object.keys(item);
                    var val = item[keys[2]];
                    var val1 = val.dataContext.valueIndex3;
                    var val2 = val.dataContext.value3;
                    //var percentage = 100 - item.values.percents;
                    var result = val2 + "<br>" + val1;
                    return result;
                },
                "id": "g3",
                "valueField": "value3",
                "title": "Rebills",
                "type": "smoothedLine",
                "lineThickness": 3,
                "bullet": "round",
                "color": "#DFAE67",
                "useDataSetColors": false
            }, {
                "balloonFunction": function (item) {
                    if (typeof balloonDisabled !== 'undefined' && balloonDisabled)
                        return;
                    var keys = Object.keys(item);
                    var val = item[keys[2]];
                    var val1 = val.dataContext.valueIndex4;
                    var val2 = val.dataContext.value4;
                    var result = val2 + "<br>" + val1;
                    return result;
                },
                "id": "g4",
                "valueField": "value4",
                "title": "Cancellations",
                "type": "smoothedLine",
                "lineThickness": 3,
                "bullet": "round",
                "lineColor": "#EC79DC",
                "useDataSetColors": false
            }],
            "valueAxes": [{
                // "includeHidden" : true,
                //  "position": "left",

                labelFunction: function (number, label, axis) {

                    var percentage = (number / axis.max * 100);

                    label = AmCharts.formatDataContextValue("[[value]] ([[percentage]]%)\n", {
                        value: number,
                        percentage: percentage.toFixed(0)
                    });

                    return label;
                }

            }],
            "stockLegend": {
                "useGraphSettings": true,
                "valueTextRegular": " ",
                "markerType": "square"
            }
        }, {
            "title": "Volume",
            "percentHeight": 30,
            "stockGraphs": [{

                "balloonFunction": function (item) {
                    if (typeof balloonDisabled !== 'undefined' && balloonDisabled)
                        return;
                    var keys = Object.keys(item);
                    var val = item[keys[2]];
                    var val1 = val.dataContext.volumeIndex;
                    var val2 = val.dataContext.volume;
                    var result = val2 + "<br>" + val1;
                    return result;
                },
                "useDataSetColors": false,
                "valueField": "volume",
                "title": "Clicks",
                "type": "column",
                "cornerRadiusTop": 2,
                "color": "#DFAE67",
                "fillAlphas": 1
            }],

            "stockLegend": {
                "useGraphSettings": true,
                "valueTextRegular": " ",
                "markerType": "none"
            }
        }],
        "valueAxis": [{ "baseValue": 10, "min": 0 }],
        "chartScrollbarSettings": {
            "graph": "g2",
            "usePeriod": "10mm",
            "position": "top"
        },

        "chartCursorSettings": {
            "valueBalloonsEnabled": true
        },

        "panelsSettings": {
            "usePrefixes": true
        },
        "balloon": {
            "fontSize" : 10
        },

        "export": {
            "enabled": true,
            "position": "bottom-right"
        }
    });


}






// ajax za chart refresh button
function refresh() {
    Metronic.blockUI({
        animate: true
    });
    $.ajax({
        url: '/statistic/chart',
        cache: false,
        data: {
            rqsInstances: $('#select_instance').select2('val'),
            rqsMerchants: $('#select_merchant').select2('val'),
            rqsServices: $('#select_service').select2('val'),
            rqsProviders: $('#select_provider').select2('val'),
            rqsCountries: $('#select_country').select2('val'),
            rqsOperators: $('#select_operator').select2('val'),

            rqsShow: $('#bs-select-show').attr('value'),
            rqsStart: $('input[name=daterangepicker_start]').val(),
            rqsEnd: $('input[name=daterangepicker_end]').val()
        },
        method: 'POST',
        success: function (response) {
            $('#chart_container').html(response);
            GenerateChart();
            Metronic.unblockUI();
        },
        error: function () {
            Metronic.unblockUI();
            errorNotification('error msg here');
        }
    });

    //Metronic.unblockUI('#mptable-portlet-body');
}



