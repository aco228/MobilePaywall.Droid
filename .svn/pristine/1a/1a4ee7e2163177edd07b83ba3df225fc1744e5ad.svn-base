.category_detail_table {
  border: 1px solid rgba(25, 118, 210, 0.13);
  border-collapse: inherit;
  padding-top: 5px;
  padding-left: 5px;
  padding-bottom: 10px;
  padding-right: 5px;
  background-color: #f7f7f7;
  border-radius: 15px;
}

.category_detail_wraper {
  margin: 20px;
  margin-bottom: 50px;
}

tr:last-child td {
  border-bottom: none;
}

.group_type{
  position:relative;
}

.group_type h4 {
  border: 1px solid rgba(25, 118, 210, 0.13);
  background-color: #f7f7f7;
  border-top: 0;
  padding: 12px;
  margin-bottom: 0px;
  padding-top: 6px;
  padding-bottom: 6px;
}

.group_type:first-of-type h4 {
  border-top: 1px solid rgba(25, 118, 210, 0.13);
}

.content_detail {
  border: 1px solid rgba(25, 118, 210, 0.13);
  background-color: #f7f7f7;
  border-top: 0;
  padding: 0px;
  padding-left:12px;
  margin-bottom: 0px;
  font-weight: 100;
  margin-left: 100px;  
}

.content_group {
  position: relative;
}

.content_detail label {
  font-size: 16px;
  line-height: 22px;
  font-weight: 500;
  margin-bottom:0;
}

.content_group_name {
  border: 1px solid rgba(25, 118, 210, 0.13);
  background-color: #f7f7f7;
  border-top: 0;
  padding: 12px;
  margin-bottom: 0px;
  margin-left: 50px;
  font-size: 16px;
  line-height: 22px;
  font-weight: 500;
  padding-top: 6px;
  padding-bottom: 6px;
}

.checkboxforcontentcategory {
  margin: 0!important;
  margin-right: 13px!important;
}

.btnUpdateContentUsageByCategory {
  z-index: 100;
  position: absolute;
  top: 0px;
  right: 4%;
}

.circleON{
  width: 15px;
  height: 15px;
  border-radius: 50%;
  background-color:green;
  position: absolute;
  top: 11px;
  right: 1%;  
}

.circleOFF{
  width: 15px;
  height: 15px;
  border-radius: 50%;
  background-color:#b9a6a6;
  position: absolute;
  top: 11px;
  right: 1%; 
}
