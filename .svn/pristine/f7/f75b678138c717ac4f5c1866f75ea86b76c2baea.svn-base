  margin: 0;
  border-bottom: 1px solid #ccc;
}
ul.nav-list li:last-child {
  border: 0px;
}
ul.nav-list li a {
  background: #FAFAFA;
  padding: 10px;
  display: block;
  color: #000000 !important;
}
ul.nav-list li a:hover {
  background: #000000;
  color: #fff !important;
}
ul.nav-list li a span {
  float: right;
  font-family: "monaco";
  font-weight: bold;
  font-size: 14px;
  line-height: 17px;
}
