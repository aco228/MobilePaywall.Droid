/* custom css */
#splash {
  background: #000000;
  width: 100%;
  height: 120%;
  position: absolute;
  top: 0;
  left: 0;
  z-index: 10000;
}
#splash #splash-content {
  margin: 0 auto;
}
#splash img#splash-bg {
  width: 100%;
  height: 100%;
  position: absolute;
  -moz-opacity: 0.6;
  opacity: 0.6;
}
#splash img#splash-title,
#splash img#splash-footer {
  position: absolute;
}
#splash img#splash-title {
  width: 141px;
  height: 120px;
  top: 50%;
  margin-top: -136px;
  left: 50%;
  margin-left: -70px;
}
#header,
#footer {
  /*width: 320px;*/

  color: #fff;
  background: #252525;
  /* Old browsers */

  /*.gradient(#555555, #000000);*/

}
header,
#header {
  /*padding: 2px 0;*/

  padding-top: 12px !important;
  padding-bottom: 0px !important;
  margin-top: -10px !important;
  margin-left: -2px !important;
  -webkit-text-shadow: 1px 1px 1px #000;
  text-shadow: 1px 1px 1px #000;
  position: fixed;
  width: 101%;
  top: -2px;
  z-index: 999999;
  /*.shadowMe(1px 1px 2px 0px #777);*/

}
header .navigation,
#header .navigation {
  display: block;
  padding-left: 7px;
}
header .navigation a,
#header .navigation a {
  display: inline-block;
  width: 19%;
  height: 100%;
  text-align: center;
  height: 50px;
}
header .navigation a img,
#header .navigation a img {
  margin-top: 6px;
  margin-bottom: 2px;
}
header .navigation a.active,
#header .navigation a.active {
  background: #000000;
}
header .navigation em,
#header .navigation em {
  color: #fff;
  font-style: normal !important;
}
#header-menu {
  /*width: 320px;*/

  color: #fff;
  background: #000000;
  /* Old browsers */

  background: -moz-linear-gradient(top, #555555 0%, #000000 100%);
  /* FF3.6+ */

  background: -webkit-gradient(linear, left top, left bottom, color-stop(0%, #555555), color-stop(100%, #000000));
  /* Chrome,Safari4+ */

  background: -webkit-linear-gradient(top, #555555 0%, #000000 100%);
  /* Chrome10+,Safari5.1+ */

  background: -o-linear-gradient(top, #555555 0%, #000000 100%);
  /* Opera 11.10+ */

  background: -ms-linear-gradient(top, #555555 0%, #000000 100%);
  /* IE10+ */

  background: linear-gradient(to bottom, #555555 0%, #000000 100%);
  /* W3C */

  filter: progid:DXImageTransform.Microsoft.gradient(startColorstr=#555555, endColorstr=#000000, GradientType=0);
  /* IE6-8 */

}
#header-menu-container {
  padding-top: 10px;
  display: none;
  height: 50px;
  text-shadow: 1px 1px 1px #000;
  position: fixed;
  width: 100%;
  top: 0;
  z-index: 999999;
  box-shadow: 1px 1px 2px 0px #777777;
  -webkit-box-shadow: 1px 1px 2px 0px #777777;
  -moz-box-shadow: 1px 1px 2px 0px #777777;
  -o-box-shadow: 1px 1px 2px 0px #777777;
  /*-ms-filter: "progid:DXImageTransform.Microsoft.Shadow(Strength=5, Direction=5, Color='#ccc')";*/

  /*filter: progid:DXImageTransform.Microsoft.Shadow(Strength=5, Direction=5, Color='#ccc');*/

  /*behavior: url(PIE.htc);*/

}
#header-menu-container .navigation {
  display: block;
  padding-left: 7px;
  color: #fff;
  background: #000000;
  /* Old browsers */

  background: -moz-linear-gradient(top, #555555 0%, #000000 100%);
  /* FF3.6+ */

  background: -webkit-gradient(linear, left top, left bottom, color-stop(0%, #555555), color-stop(100%, #000000));
  /* Chrome,Safari4+ */

  background: -webkit-linear-gradient(top, #555555 0%, #000000 100%);
  /* Chrome10+,Safari5.1+ */

  background: -o-linear-gradient(top, #555555 0%, #000000 100%);
  /* Opera 11.10+ */

  background: -ms-linear-gradient(top, #555555 0%, #000000 100%);
  /* IE10+ */

  background: linear-gradient(to bottom, #555555 0%, #000000 100%);
  /* W3C */

  filter: progid:DXImageTransform.Microsoft.gradient(startColorstr=#555555, endColorstr=#000000, GradientType=0);
  /* IE6-8 */

}
#header-menu-container .navigation a {
  display: inline-block;
  width: 24% !important;
  height: 100%;
  text-align: center;
  height: 50px;
}
#header-menu-container .navigation a img {
  margin-top: 6px;
  margin-bottom: 2px;
}
#header-menu-container .navigation a.active {
  background: #000000;
}
#header-menu-container .navigation em {
  color: #fff;
  font-style: normal !important;
}
/* ==============================*/
.video {
  /* .shadowMe(1px 1px 9px 0px #444); */

  margin-bottom: 10px;
  padding-top: 10px;
}
.video .video-header {
  padding: 4%;
  background: rgba(255, 255, 255, 0.7);
  border-bottom: 1px solid #bbb;
}
.video .video-header .title {
  text-transform: uppercase;
  color: #000000;
  margin: 0 !important;
}
.video .video-header .info {
  color: #666;
  margin: 0 !important;
}
.video .video-header .info span {
  color: #aaa;
}
.video .video-body {
  padding: 4%;
  padding-top: 10px;
  padding-bottom: 0;
}
.video .video-body img {
  float: right;
  margin: 0;
  padding-right: 10px;
  width: 128px;
  height: auto;
}
.video .video-body img.left {
  float: left;
  width: 100%;
  max-width: 130px;
  min-width: 128px;
  z-index: 1;
}
.video .video-body img.right {
  float: right;
}
.video .video-body .icon-text {
  display: block;
  margin-bottom: 20px;
  min-height: 86px;
}
.video .video-body p {
  margin: 0 0 15px 0;
  font-size: 90%;
}
.video .video-body p strong {
  color: #000000;
  font-weight: normal;
  font-size: 120%;
  display: inline;
  padding-bottom: 3px;
}
.video .video-footer {
  padding: 2%;
  padding-left: 4%;
  padding-bottom: 40px;
}
.video .video-footer .social {
  margin-top: 3px;
}
