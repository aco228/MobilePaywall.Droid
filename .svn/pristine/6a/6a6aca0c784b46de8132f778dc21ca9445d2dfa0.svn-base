using System;
using System.Collections.Generic;
using System.Linq;
using System.Web;

namespace MobilePaywall.Web.Models
{
	public class CallbackResultModel
	{
		private bool _isCompleted;
    private string _requestUrl = string.Empty;
		private Guid _callbackGuid;
    private string _fatal = string.Empty;
    
    public bool IsCompleted
    {
      get { return this._isCompleted; }
      set { this._isCompleted = value; }
    }

    public string RequestUrl
    {
      get { return this._requestUrl; }
      set { this._requestUrl = value; }
    }

		public Guid CallbackGuid
		{
			get { return this._callbackGuid; }
			set { this._callbackGuid = value; }
		}

    public string Fatal
    {
      get { return this._fatal; }
      set { this._fatal = value; }
    }

		

	}
}